            "blockIdx": 18
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（のろのろのろ～）",
            "enText": "(Sluggishly)",
            "nextBlock": 20,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 67,
                    "pathId": 889004167152236731
                },
