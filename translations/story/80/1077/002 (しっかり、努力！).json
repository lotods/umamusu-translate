            "nextBlock": 7,
            "origClipLength": 89,
            "choices": [
                {
                    "jpText": "細かい動きは集中力を使うもんな",
                    "enText": "Fine movement takes a lot of \nconcentration, doesn't it?",
                    "nextBlock": 7
                },
