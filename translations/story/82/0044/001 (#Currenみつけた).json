            "pathId": 9031434843691587136,
            "blockIdx": 15
        },
        {
            "jpName": "カレンのファンたち",
            "enName": "",
            "jpText": "は～い！",
            "enText": "Yes!",
            "nextBlock": 17,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 61,
                    "pathId": -2713485246900788027
                },
                {
                    "origLen": 61,
                    "pathId": 7224706519524751375
