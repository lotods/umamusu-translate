            "pathId": 1923215342670061328,
            "blockIdx": 19
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "え……でも……。",
            "enText": "Eh…… even ……",
            "nextBlock": 21,
            "origClipLength": 55,
            "choices": [
                {
                    "jpText": "きっと大丈夫だから",
                    "enText": "It's going to be okay.",
                    "nextBlock": 21
