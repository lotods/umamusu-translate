            "pathId": -8554630186827161567,
            "blockIdx": 31
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "……ブライアンさん。",
            "enText": "…Brian-san.",
            "nextBlock": 33,
            "origClipLength": 120,
            "animData": [
                {
                    "origLen": 50,
                    "pathId": -3543173070968035923
                },
                {
                    "origLen": 57,
                    "pathId": 5558775138029821591
                }
