            "pathId": -7135149771885617300,
            "blockIdx": 14
        },
        {
            "jpName": "<username>",
            "enName": "",
            "jpText": "スズカ……っ！？",
            "enText": "Suzuka…?!",
            "nextBlock": 16,
            "origClipLength": 30,
            "animData": [
                {
                    "origLen": 59,
                    "pathId": 680254011073224659
                },
                {
                    "origLen": 11,
