            "pathId": 6808190042658120827,
            "blockIdx": 26
        },
        {
            "jpName": "<username>",
            "enName": "",
            "jpText": "なるほど……。",
            "enText": "Okay…",
            "nextBlock": 28,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 150,
                    "pathId": -3213071971518492957
                }
            ],
            "pathId": 2582750283540829954,
            "blockIdx": 27
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "…………。",
            "enText": "…",
            "nextBlock": 29,
