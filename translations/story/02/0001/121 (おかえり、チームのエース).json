            "pathId": 3285944957798596038,
            "blockIdx": 12
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "…………。",
            "enText": "…",
            "nextBlock": 14,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 212,
                    "pathId": 8185097142525169154
