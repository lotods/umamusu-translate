            "blockIdx": 12
        },
        {
            "jpName": "デアリングハート",
            "enName": "Daring Heart",
            "jpText": "――ァアアアアアアッ！！",
            "enText": "――Aaaaaaaaaah!!",
            "nextBlock": 14,
            "origClipLength": 105,
            "voiceIdx": 12,
