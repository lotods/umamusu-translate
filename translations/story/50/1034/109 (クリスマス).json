            "pathId": -8397534465339515413,
            "blockIdx": 20
        },
        {
            "jpName": "イナリワン",
            "enName": "Inari One",
            "jpText": "にしても――",
            "enText": "but―",
            "nextBlock": 22,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 54,
                    "pathId": 8546115813067949680
