            "pathId": 5620955212820245639,
            "blockIdx": 14
        },
        {
            "jpName": "イナリワン",
            "enName": "Inari One",
            "jpText": "よっ……と！",
            "enText": "Yo…… and!",
            "nextBlock": 16,
            "origClipLength": 52,
            "choices": [
                {
                    "jpText": "まだ動かない方がいいんじゃ……",
                    "enText": "It's better not to move yet. ……",
                    "nextBlock": 16
                }
