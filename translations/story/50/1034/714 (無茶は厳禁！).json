            "pathId": 7702446820786941203,
            "blockIdx": 14
        },
        {
            "jpName": "イナリワン",
            "enName": "Inari One",
            "jpText": "1、2、3……くぅっ！",
            "enText": "One, two, three…… duh!",
            "nextBlock": 16,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 31,
                    "pathId": -1026973748777344848
