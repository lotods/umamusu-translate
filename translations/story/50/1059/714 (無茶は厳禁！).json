            "blockIdx": 12
        },
        {
            "jpName": "メジロドーベル",
            "enName": "Mejiro Dober",
            "jpText": "いっ……ッッ！！",
            "enText": "I am ……!!!!",
            "nextBlock": 14,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 8,
                    "pathId": 4821980789415816218
