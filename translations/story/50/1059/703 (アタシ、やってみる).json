                    "origLen": 122,
                    "pathId": -3429012630974341250
                },
                {
                    "origLen": 158,
                    "pathId": 3302073642881634992
                }
            ],
            "pathId": 2536971068072253225,
            "blockIdx": 7
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "……ああ、ドーベルか。",
            "enText": "…… Oh, Dobel.",
            "nextBlock": 9,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 158,
                    "pathId": 5410228392349987202
                },
                {
                    "origLen": 70,
                    "pathId": 6410743743005138559
