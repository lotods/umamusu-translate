            "blockIdx": 17
        },
        {
            "jpName": "メジロドーベル",
            "enName": "Mejiro Dober",
            "jpText": "え？　……なに、どういう意味？",
            "enText": "Huh?…… What, what do you mean?",
            "nextBlock": 19,
            "origClipLength": 64,
            "choices": [
                {
                    "jpText": "あ、いや――",
                    "enText": "Oh, no―",
                    "nextBlock": 19
                }
            ],
            "animData": [
                {
                    "origLen": 37,
                    "pathId": 3056066007275047920
                },
                {
                    "origLen": 75,
                    "pathId": 3833520573797210786
                }
