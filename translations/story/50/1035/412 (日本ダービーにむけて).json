    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "ついにやってきた『日本ダービー』",
            "enText": "The \"Japan Derby\" has finally arrived!",
            "nextBlock": 2,
            "origClipLength": 65,
            "animData": [
                {
                    "origLen": 118,
