            "blockIdx": 15
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "……らしくないと思った？",
            "enText": "Didn't seem like…… to you?",
            "nextBlock": 17,
            "origClipLength": 60,
            "choices": [
                {
                    "jpText": "そんなことない！頑張ろう！",
                    "enText": "No, it's not! Let's go for it!",
                    "nextBlock": 17
                },
                {
                    "jpText": "いい練習になると思う",
                    "enText": "I think it would be good practice.",
                    "nextBlock": 24
                }
            ],
            "animData": [
                {
                    "origLen": 136,
                    "pathId": -6268178940340348910
                },
                {
                    "origLen": 142,
                    "pathId": -3186688398839213095
                }
