            "pathId": -1921833008088982791,
            "blockIdx": 30
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "ずるいずるーい！　ネイチャちゃんったら\r\nマヤの知らないとこで特訓してたでしょー！",
            "enText": "No fair! No fair! Necha, you've been training \nwithout Maya's knowledge!",
            "nextBlock": 32,
            "origClipLength": 98,
            "animData": [
                {
                    "origLen": 130,
                    "pathId": -8267544927443368502
                },
                {
                    "origLen": 59,
                    "pathId": 5189864648563277802
                },
                {
                    "origLen": 109,
                    "pathId": 7309784584720552282
                },
                {
                    "origLen": 71,
                    "pathId": 1761842013156602301
                },
                {
                    "origLen": 109,
                    "pathId": -3657853065075161990
                }
            ],
            "pathId": -9111338454550633858,
            "blockIdx": 31
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "いやいや、アンタもアタシの知らないとこで\r\n走ってるでしょうに。",
            "enText": "No, no, no, you're probably running around \nwithout my knowledge.",
