        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "……アタシも頑張らなきゃな。",
            "enText": "…… I'll have to work on my Atari.",
            "nextBlock": 30,
            "origClipLength": 63,
            "choices": [
                {
                    "jpText": "ネイチャ？",
                    "enText": "Natya?",
                    "nextBlock": 30
                }
