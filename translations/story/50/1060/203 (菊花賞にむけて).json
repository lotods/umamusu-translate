            "pathId": 4955519094287889159,
            "blockIdx": 28
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "……っ。",
            "enText": "……",
            "nextBlock": 30,
            "origClipLength": 50,
            "animData": [
