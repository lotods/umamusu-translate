            "pathId": -8873390506207083860,
            "blockIdx": 32
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "……～～っ。",
            "enText": "……～～.",
            "nextBlock": 34,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 15,
                    "pathId": 3820207178379277954
