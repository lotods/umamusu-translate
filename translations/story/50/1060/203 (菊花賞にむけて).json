            "pathId": -235328038204068214,
            "blockIdx": 13
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "えっ……！？\r\nテイオーが！？",
            "enText": "What? ……! Teo!",
            "nextBlock": 15,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 43,
                    "pathId": 5701812917533924405
                },
                {
                    "origLen": 75,
                    "pathId": -7598497111425597222
                }
