            "pathId": -2620401284743322576,
            "blockIdx": 2
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "ですなー。",
            "enText": "I guess.",
            "nextBlock": 4,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 30,
                    "pathId": -8877580677541766231
                },
                {
                    "origLen": 20,
