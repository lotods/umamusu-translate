            "nextBlock": 3,
            "origClipLength": 85,
            "pathId": -6649259363389402315,
            "blockIdx": 2
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "……。",
            "enText": "……",
            "nextBlock": 4,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "そろそろ準備しよう",
                    "enText": "It's time to get ready.",
                    "nextBlock": 4
