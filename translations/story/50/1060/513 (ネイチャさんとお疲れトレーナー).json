            "animData": [
                {
                    "origLen": 78,
                    "pathId": 4093455899089248791
                },
                {
                    "origLen": 93,
                    "pathId": -424044984759256471
                }
            ],
            "pathId": -537358775906154123,
            "blockIdx": 11
        },
        {
            "jpName": "ナイスネイチャ",
            "enName": "Nice Nature",
            "jpText": "さあさあ、冷めないうちに\r\nちゃっちゃと召しあがってくださいな。",
            "enText": "Now, now, please enjoy it quickly \nbefore it gets cold.",
            "nextBlock": 13,
            "origClipLength": 86,
            "choices": [
                {
                    "jpText": "すごい、お店のメニューみたいだ！",
                    "enText": "Great, looks like a restaurant menu!",
                    "nextBlock": 13
                },
                {
                    "jpText": "すごい、お店のメニューみたい！",
                    "enText": "Wow, looks like a restaurant menu!",
                    "nextBlock": 13
                }
