        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "子犬……？",
            "enText": "ZiDog ……?",
            "nextBlock": 9,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 80,
                    "pathId": -1120336263602025805
                },
