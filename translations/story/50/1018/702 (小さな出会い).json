            "pathId": -4273343232955312514,
            "blockIdx": 6
        },
        {
            "jpName": "子犬",
            "enName": "",
            "jpText": "くぅー……くぅーん……。",
            "enText": "Kuu…… Kuu……",
            "nextBlock": 8,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 121,
