            "pathId": -8442368735060057086,
            "blockIdx": 16
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "……ふっ。",
            "enText": "…… phew.",
            "nextBlock": 18,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "よかったな",
                    "enText": "Good for you.",
                    "nextBlock": 18
                },
                {
                    "jpText": "よかったね",
                    "enText": "Good for you.",
                    "nextBlock": 18
                }
            ],
            "animData": [
                {
                    "origLen": 102,
                    "pathId": 8948991519485633878
                },
                {
                    "origLen": 115,
