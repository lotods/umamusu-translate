                }
            ],
            "pathId": 4052308567199361845,
            "blockIdx": 17
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ワアアアアアアーーーッ！！！）",
            "enText": "(Waaaaaaaaaaahhh!!!)",
            "nextBlock": 19,
            "origClipLength": 121,
            "animData": [
                {
                    "origLen": 132,
