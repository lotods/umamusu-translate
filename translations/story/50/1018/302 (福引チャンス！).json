            "animData": [
                {
                    "origLen": 101,
                    "pathId": 611418550000819459
                },
                {
                    "origLen": 101,
                    "pathId": 7975786265991444384
                }
            ],
            "pathId": -1845663298918227084,
            "blockIdx": 3
        },
        {
            "jpName": "商店街の人",
            "enName": "Market Person",
            "jpText": "1等は『特上にんじんハンバーグ』\r\n2等は『にんじん山盛り』\n3等は『にんじん1本』！",
            "enText": "First prize is a \"Special carrot hamburger \nsteak,\" second prize is a \"pile of carrots,\" \nand third prize is \"a carrot!\"",
            "nextBlock": 5,
            "origClipLength": 101,
            "animData": [
                {
                    "origLen": 62,
                    "pathId": 7913484237515986514
                },
                {
                    "origLen": 112,
                    "pathId": -3224864004320943557
