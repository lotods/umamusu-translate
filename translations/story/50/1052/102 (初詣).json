            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 17,
                    "pathId": 5741260662016512336
                },
                {
                    "origLen": 70,
                    "pathId": -6856672562076812117
                }
            ],
            "pathId": -3104028378577511663,
            "blockIdx": 6
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "わたし、いっぱいお願いしたいことあるんだ！\r\n『有馬記念に出たい』でしょー！",
            "enText": "I have a lot to ask of you! I want to be in \nthe Arimakinen!",
            "nextBlock": 8,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 42,
                    "pathId": 6939251158195673740
                },
                {
                    "origLen": 80,
                    "pathId": -8145252047483831932
