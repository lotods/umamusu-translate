            "blockIdx": 9
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "うんっ！\r\n行ってきます！",
            "enText": "Yeah! I'm off!",
            "nextBlock": 11,
            "origClipLength": 53,
            "animData": [
                {
                    "origLen": 39,
