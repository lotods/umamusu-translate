            "blockIdx": 1
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "はふう～。\nちょっとくたびれちゃった～。",
            "enText": "I'm a little tired～. I'm a little tired～.",
            "nextBlock": 3,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 26,
                    "pathId": 7472778471207536667
                },
                {
                    "origLen": 69,
