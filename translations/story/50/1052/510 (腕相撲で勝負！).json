            "blockIdx": 12
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "あっ、そっかあ！　そうだよね！！",
            "enText": "Oh, I see! That's right!",
            "nextBlock": 14,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 60,
                    "pathId": -3028862680338010498
                },
                {
                    "origLen": 238,
                    "pathId": -2812127406305481419
