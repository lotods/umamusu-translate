            "pathId": 8949692105538219280,
            "blockIdx": 10
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "……もう寝た？",
            "enText": "…… Have you gone to bed yet?",
            "nextBlock": 12,
            "origClipLength": 49,
            "choices": [
                {
                    "jpText": "……",
                    "enText": "……",
                    "nextBlock": 12
                }
            ],
            "pathId": -5226552945466000023,
            "blockIdx": 11
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "えへへっ。トレーナー、ぐっすりだ。\nよーし……！",
            "enText": "Heh heh heh. Trainers, sleep \ntight. Alright, ……!",
            "nextBlock": 13,
            "origClipLength": 60,
            "pathId": 4191284734448765054,
            "blockIdx": 12
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "にんじんケーキが……お空から……\n1000個も降ってきちゃった……！",
