            "blockIdx": 5
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "……～～っぶはぁ！！",
            "enText": "……～～～buhahaha!",
            "nextBlock": 7,
            "origClipLength": 51,
            "animData": [
                {
