        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "ね！　安心した？",
            "enText": "Hey! Are you relieved?",
            "nextBlock": 4,
            "origClipLength": 50,
            "choices": [
                {
                    "jpText": "じ、じっとして……",
                    "enText": "Hold still, hold still: ……",
                    "nextBlock": 4
                },
                {
                    "jpText": "コラッ！安静に！",
                    "enText": "Hey! Hey! Rest easy!",
                    "nextBlock": 9
