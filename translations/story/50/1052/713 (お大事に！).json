            "pathId": 5279365969395934444,
            "blockIdx": 9
        },
        {
            "jpName": "ハルウララ",
            "enName": "Haru Urara",
            "jpText": "トレーナー……！",
            "enText": "Trainer ……!",
            "nextBlock": 11,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 26,
                    "pathId": 3079198758297253013
                },
                {
                    "origLen": 61,
                    "pathId": -43288952135541020
                }
