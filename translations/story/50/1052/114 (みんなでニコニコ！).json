            "nextBlock": 3,
            "origClipLength": 63,
            "choices": [
                {
                    "jpText": "トレーニング、頑張ろうな",
                    "enText": "Good luck with your training.",
                    "nextBlock": 3
                },
                {
                    "jpText": "トレーニング、頑張ろうね",
                    "enText": "Good luck with your training!",
                    "nextBlock": 3
