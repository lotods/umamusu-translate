            "enText": "We propose to reconfigure the \nmenu based on this data.",
            "nextBlock": 20,
            "origClipLength": 80,
            "choices": [
                {
                    "jpText": "任せろ！",
                    "enText": "I got it!",
                    "nextBlock": 20
                },
                {
                    "jpText": "任せて！",
                    "enText": "I got it!",
                    "nextBlock": 20
                }
            ],
            "animData": [
