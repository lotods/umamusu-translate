            "blockIdx": 14
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ドンガラガッシャーーン！）",
            "enText": "(DONG RING RING! )",
            "nextBlock": 16,
            "origClipLength": 63,
            "choices": [
                {
                    "jpText": "ブルボン！？",
                    "enText": "Bourbon!",
                    "nextBlock": 16
