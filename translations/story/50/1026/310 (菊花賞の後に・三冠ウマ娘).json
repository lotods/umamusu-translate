            "pathId": 6822052105077519316,
            "blockIdx": 10
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "オーダー、完了しました……！\nマスター……！",
            "enText": "Order, complete…! Master…!",
            "nextBlock": 12,
            "origClipLength": 73,
            "choices": [
                {
                    "jpText": "よく頑張った！",
                    "enText": "Well done!",
                    "nextBlock": 12
                },
                {
                    "jpText": "よく頑張ったね！",
                    "enText": "You did good!",
                    "nextBlock": 12
                }
            ],
            "animData": [
                {
                    "origLen": 13,
                    "pathId": 3630243132207763975
                },
                {
                    "origLen": 106,
                    "pathId": -7191243312202663043
                },
                {
                    "origLen": 115,
                    "pathId": -1933840654243521055
                }
