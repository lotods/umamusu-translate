            "pathId": 5869427977555268748,
            "blockIdx": 7
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "やりました……お父さん……、\nマスター……っ！",
            "enText": "I did it… Dad… Master…!",
            "nextBlock": 9,
            "origClipLength": 75,
            "animData": [
                {
                    "origLen": 81,
                    "pathId": 4861440427199416205
                },
