            "pathId": -4067682601164787786,
            "blockIdx": 7
        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "ただいま帰還しました。マスター。",
            "enText": "I have just returned. Master.",
            "nextBlock": 9,
            "origClipLength": 65,
            "choices": [
                {
                    "jpText": "とってもよかった！",
                    "enText": "Very good!",
                    "nextBlock": 9
                },
                {
                    "jpText": "とってもよかったよ！",
                    "enText": "It was very good!",
                    "nextBlock": 9
                }
            ],
            "animData": [
                {
                    "origLen": 15,
                    "pathId": -2144037515041657753
                },
                {
                    "origLen": 15,
                    "pathId": 1719620534785235942
