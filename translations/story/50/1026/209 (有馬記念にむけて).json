        },
        {
            "jpName": "ミホノブルボン",
            "enName": "Mihono Bourbon",
            "jpText": "本日は必ず……勝利します。",
            "enText": "We will definitely win… today.",
            "nextBlock": 10,
            "origClipLength": 61,
            "choices": [
                {
                    "jpText": "ああ！",
                    "enText": "Aah!",
                    "nextBlock": 10
                },
                {
                    "jpText": "うん！",
                    "enText": "Yeah!",
                    "nextBlock": 10
