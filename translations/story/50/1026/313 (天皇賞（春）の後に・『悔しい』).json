        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ウワァアアアアア――！）",
            "enText": "(Whoa, whoa, whoa, whoa! )",
            "nextBlock": 6,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 18,
                    "pathId": 5722375913419945130
                },
                {
                    "origLen": 17,
