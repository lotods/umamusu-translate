            "pathId": -5727093599436035854,
            "blockIdx": 1
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……。",
            "enText": "……",
            "nextBlock": 3,
            "origClipLength": 48,
            "animData": [
