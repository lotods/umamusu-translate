            "blockIdx": 1
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ピシッ）",
            "enText": "(Pish)",
            "nextBlock": 3,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "いたっ！？",
                    "enText": "There it is!",
                    "nextBlock": 3
                }
            ],
            "animData": [
                {
                    "origLen": 62,
                    "pathId": 1354522670844582668
                },
                {
                    "origLen": 62,
                    "pathId": -3630630236660206910
