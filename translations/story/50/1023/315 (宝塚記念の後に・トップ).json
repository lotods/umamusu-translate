        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ブロロロロロロ……）",
            "enText": "(Bro. ……)",
            "nextBlock": 22,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 43,
