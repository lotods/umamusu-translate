            "pathId": -1789660060832952411,
            "blockIdx": 16
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "あっ！",
            "enText": "Ah!",
            "nextBlock": 18,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 102,
                    "pathId": 3532409745617237686
                },
                {
                    "origLen": 102,
