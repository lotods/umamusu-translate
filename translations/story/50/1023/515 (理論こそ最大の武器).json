            "pathId": -4951093693356047778,
            "blockIdx": 3
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "そうか？　悪いな。",
            "enText": "Really? Sorry.",
            "nextBlock": 5,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 38,
                    "pathId": 208031444716035598
                },
                {
                    "origLen": 67,
                    "pathId": -5058891899680062861
