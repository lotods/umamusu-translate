            "pathId": -152730537269711112,
            "blockIdx": 4
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "確か、あちらに置いたはず――",
            "enText": "I believe I put it over there―",
            "nextBlock": 6,
            "origClipLength": 63,
            "animData": [
                {
                    "origLen": 91,
                    "pathId": -7758001939315702425
                },
                {
                    "origLen": 90,
                    "pathId": -7545132249841807561
