            "blockIdx": 9
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "ふっ……当然だ。",
            "enText": "Phew…… of course.",
            "nextBlock": 11,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 44,
                    "pathId": 4158047493657278975
