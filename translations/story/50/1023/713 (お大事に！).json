            "blockIdx": 8
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "……君には敵わないな。",
            "enText": "I'm no match for you, ……",
            "nextBlock": 10,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 28,
                    "pathId": 1439107888250497566
                },
                {
                    "origLen": 70,
                    "pathId": 2605331433029722619
                }
