            "pathId": 2781708235793402686,
            "blockIdx": 14
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "……はい。",
            "enText": "… Yes.",
            "nextBlock": 16,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 8461584906091582770
