            "pathId": -3824262702898121062,
            "blockIdx": 1
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ヒュウウウウ……！）",
            "enText": "(Huuuuuu…!)",
            "nextBlock": 3,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 7043698874034278462
