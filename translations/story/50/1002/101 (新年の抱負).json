            "pathId": 3156249641726223213,
            "blockIdx": 12
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "速さ、ですか……。",
            "enText": "Speed, or ……",
            "nextBlock": 14,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 33,
                    "pathId": -335886778844744455
