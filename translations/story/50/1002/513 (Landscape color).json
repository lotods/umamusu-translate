            "blockIdx": 18
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "はい、そうです。",
            "enText": "Yes, it is.",
            "nextBlock": 20,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 38,
                    "pathId": 9189583864380404739
                },
                {
                    "origLen": 61,
