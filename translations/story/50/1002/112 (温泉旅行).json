            "animData": [
                {
                    "origLen": 82,
                    "pathId": -5210919226775465878
                },
                {
                    "origLen": 109,
                    "pathId": 9124728090547028099
                }
            ],
            "pathId": 5246590512199289890,
            "blockIdx": 17
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "ホントですか……？",
