            "pathId": -5349386584166321486,
            "blockIdx": 7
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "……はぁ……。",
            "enText": "…… is a ……",
            "nextBlock": 9,
            "origClipLength": 49,
            "voiceIdx": 60,
            "choices": [
                {
                    "jpText": "大丈夫？",
                    "enText": "Great man?",
