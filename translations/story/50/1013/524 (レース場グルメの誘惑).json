            "pathId": 7367530106420994155,
            "blockIdx": 25
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "はっ……！　イクノさん！",
            "enText": "Haha ……! Ikuno-san!",
            "nextBlock": 27,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 89,
