            "pathId": -8247818276718783136,
            "blockIdx": 19
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "…………。",
            "enText": "…………",
            "nextBlock": 21,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 89,
                    "pathId": -5845863563385193401
