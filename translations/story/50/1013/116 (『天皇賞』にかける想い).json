            "pathId": -2134017043590102286,
            "blockIdx": 24
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "……ええ。",
            "enText": "…… yeah.",
            "nextBlock": 26,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 83,
                    "pathId": 551391764695882930
                },
                {
                    "origLen": 59,
                    "pathId": -389325441557723312
                }
