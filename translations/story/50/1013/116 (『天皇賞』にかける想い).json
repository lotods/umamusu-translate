            "blockIdx": 24
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "……ええ。",
            "enText": "…… yeah.",
            "nextBlock": 26,
            "origClipLength": 48,
