        {
            "jpName": "スイープトウショウ",
            "enName": "Sweep Tosho",
            "jpText": "…………。",
            "enText": "…………",
            "nextBlock": 5,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "どうしたんだ？スイープ",
                    "enText": "What's going on? Sweep.",
                    "nextBlock": 5
                },
                {
                    "jpText": "どうしたの？スイープ",
                    "enText": "What's up? Sweep.",
