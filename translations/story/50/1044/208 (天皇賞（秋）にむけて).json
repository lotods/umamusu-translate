            "blockIdx": 22
        },
        {
            "jpName": "カワカミプリンセス",
            "enName": "Kawakami Princess",
            "jpText": "……えっと、私は……。",
            "enText": "…… Well, I'm ……",
            "nextBlock": 24,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 31,
                    "pathId": -4715825871272351660
                },
                {
                    "origLen": 70,
                    "pathId": 7488432345010573739
                }
