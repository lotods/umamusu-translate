            "pathId": -7253613852544135345,
            "blockIdx": 16
        },
        {
            "jpName": "スイープトウショウ",
            "enName": "Sweep Tosho",
            "jpText": "いたぁ……。",
            "enText": "Ita: ……",
            "nextBlock": 18,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 43,
                    "pathId": 3272763662022576569
                },
                {
                    "origLen": 63,
