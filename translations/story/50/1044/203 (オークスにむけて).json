            "pathId": 8727844033143157962,
            "blockIdx": 19
        },
        {
            "jpName": "スイープトウショウ",
            "enName": "Sweep Tosho",
            "jpText": "！",
            "enText": "Oh, my God!",
            "nextBlock": 21,
            "origClipLength": 46,
