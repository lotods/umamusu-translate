            "blockIdx": 14
        },
        {
            "jpName": "スイープトウショウ",
            "enName": "Sweep Tosho",
            "jpText": "ざぁんねん♪\nそれじゃ、花マルはあげられないわね！！",
            "enText": "Zanen ♪ Then I can't give you flower marbles!",
            "nextBlock": 16,
            "origClipLength": 78,
            "choices": [
                {
                    "jpText": "えっ！？",
                    "enText": "What?",
                    "nextBlock": 16
                }
            ],
            "animData": [
                {
                    "origLen": 46,
                    "pathId": 5601636814481198401
                },
                {
                    "origLen": 89,
                    "pathId": 7622906834779748268
                }
