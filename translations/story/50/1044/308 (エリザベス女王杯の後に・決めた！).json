            "pathId": 1649684885260104101,
            "blockIdx": 13
        },
        {
            "jpName": "スイープトウショウ",
            "enName": "Sweep Tosho",
            "jpText": "で・も～……。",
            "enText": "So… but～……",
            "nextBlock": 15,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 41,
                    "pathId": -7454601972452035394
