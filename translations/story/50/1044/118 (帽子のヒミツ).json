        },
        {
            "jpName": "スイープトウショウ",
            "enName": "Sweep Tosho",
            "jpText": "きゃああっ！？",
            "enText": "Aaah!",
            "nextBlock": 16,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 118,
