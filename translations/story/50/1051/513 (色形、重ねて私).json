        {
            "jpName": "ニシノフラワー",
            "enName": "Nishino Flower",
            "jpText": "あ――そっか。",
            "enText": "Oh―I see.",
            "nextBlock": 24,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 38,
                    "pathId": -401362497179096968
