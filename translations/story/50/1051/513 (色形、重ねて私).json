            "blockIdx": 27
        },
        {
            "jpName": "ニシノフラワー",
            "enName": "Nishino Flower",
            "jpText": "ありがとうございますっ！",
            "enText": "Thank you!",
            "nextBlock": 29,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 74,
                    "pathId": 1339005472059743598
                },
