        },
        {
            "jpName": "ニシノフラワー",
            "enName": "Nishino Flower",
            "jpText": "ちゃんと戻ってました！\nぶいっ……です♪",
            "enText": "I was right back! This is ………",
            "nextBlock": 3,
            "origClipLength": 71,
            "choices": [
                {
                    "jpText": "おめでとう！",
                    "enText": "Congratulations!",
                    "nextBlock": 3
                }
            ],
            "animData": [
