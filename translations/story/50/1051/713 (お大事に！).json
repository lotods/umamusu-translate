            "pathId": 6423727936458297502,
            "blockIdx": 5
        },
        {
            "jpName": "ニシノフラワー",
            "enName": "Nishino Flower",
            "jpText": "ですけど……私は……。",
            "enText": "But…… I am ……",
            "nextBlock": 7,
            "origClipLength": 59,
            "choices": [
                {
                    "jpText": "リスクの少ない選択を取ろう",
                    "enText": "Let's take the low-risk option.",
                    "nextBlock": 7
                }
            ],
            "animData": [
                {
                    "origLen": 39,
                    "pathId": -6057121483985178948
                },
                {
                    "origLen": 70,
                    "pathId": -7850193109444513813
                }
