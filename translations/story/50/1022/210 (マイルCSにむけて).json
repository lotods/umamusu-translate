    "title": "マイルCSにむけて",
    "text": [
        {
            "jpName": "ファインモーション",
            "enName": "Fine Motion",
            "jpText": "――夢じゃない。",
            "enText": "―It's not a dream.",
            "nextBlock": 2,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 66,
                    "pathId": -7534123780982754117
                },
                {
                    "origLen": 66,
