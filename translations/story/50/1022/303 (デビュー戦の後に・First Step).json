            "pathId": -4148347485927934282,
            "blockIdx": 14
        },
        {
            "jpName": "ファインモーション",
            "enName": "Fine Motion",
            "jpText": "むむう、確かに……。",
            "enText": "Mmmm, surely ……",
            "nextBlock": 16,
            "origClipLength": 63,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 224564480695067880
