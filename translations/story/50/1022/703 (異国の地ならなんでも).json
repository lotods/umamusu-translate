            "origClipLength": 84,
            "choices": [
                {
                    "jpText": "次のはずだ",
                    "enText": "Should be next.",
                    "nextBlock": 6
                },
                {
                    "jpText": "次のはずだよ",
                    "enText": "Should be next.",
                    "nextBlock": 6
