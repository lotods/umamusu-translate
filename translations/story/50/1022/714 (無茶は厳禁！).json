            "origClipLength": 97,
            "choices": [
                {
                    "jpText": "俺も注意不足だった……",
                    "enText": "I wasn't paying attention either: ……",
                    "nextBlock": 2
                },
                {
                    "jpText": "私も注意不足だった……",
                    "enText": "I wasn't paying attention either: ……",
                    "nextBlock": 2
                }
