            "origClipLength": 78,
            "choices": [
                {
                    "jpText": "そんなことはないぞ",
                    "enText": "That's not true.",
                    "nextBlock": 8
                },
                {
                    "jpText": "そんなことはないよ",
                    "enText": "That's not true.",
                    "nextBlock": 8
                }
