            "pathId": -3666838056128091839,
            "blockIdx": 26
        },
        {
            "jpName": "メジロアルダン",
            "enName": "Mejiro Ardan",
            "jpText": "えっ？",
            "enText": "What?",
            "nextBlock": 28,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 31,
                    "pathId": 6445557397976172259
                },
                {
                    "origLen": 59,
                    "pathId": -3993864372813071132
                }
