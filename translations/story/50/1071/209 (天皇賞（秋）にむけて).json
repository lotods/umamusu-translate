            "pathId": -2125142874785308655,
            "blockIdx": 9
        },
        {
            "jpName": "メジロアルダン",
            "enName": "Mejiro Ardan",
            "jpText": "はい。\n行ってきます。",
            "enText": "Yes, sir. I'm off.",
            "nextBlock": 11,
            "origClipLength": 59,
            "voiceIdx": 138,
            "animData": [
                {
                    "origLen": 73,
                    "pathId": 5239823745491275106
