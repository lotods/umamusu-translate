            "blockIdx": 25
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "もちろん！　ばっちりエルの大勝利デース！！",
            "enText": "Of course! Elle won big time!",
            "nextBlock": 27,
            "origClipLength": 72,
            "animData": [
                {
                    "origLen": 38,
                    "pathId": -1523590351386568618
                },
                {
                    "origLen": 83,
                    "pathId": -5331552486917635555
                },
