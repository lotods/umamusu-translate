            "pathId": -5268288023338639974,
            "blockIdx": 4
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "El Condor Pasa",
            "jpText": "常に自分にとって苦しい道を選び続けること。",
            "enText": "Always continue to choose the path that is \npainful for you.",
            "nextBlock": 6,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 21,
                    "pathId": 5038150649805362906
                },
                {
                    "origLen": 69,
                    "pathId": -5996940554449279724
                }
