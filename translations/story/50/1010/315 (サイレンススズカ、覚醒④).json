            "blockIdx": 8
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "ヤッフー！\nまたまたスズカのヴィクトリー！",
            "enText": "Yahoo!!! Another tinny Victory!",
            "nextBlock": 10,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 44,
                    "pathId": 4785653919030939781
                },
                {
                    "origLen": 69,
