                }
            ],
            "pathId": 7614760088676205309,
            "blockIdx": 1
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "エ～……折り畳み傘デスカ～？",
            "enText": "E～…… folding umbrella?",
            "nextBlock": 3,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 30,
                    "pathId": -1897993574656726823
                },
                {
                    "origLen": 65,
                    "pathId": -613167492229786602
