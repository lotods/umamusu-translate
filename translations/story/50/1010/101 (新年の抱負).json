    "title": "新年の抱負",
    "text": [
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "アケマシテオメデトウでございマス！",
            "enText": "Akemashite omedetou masu!",
            "nextBlock": 2,
            "origClipLength": 75,
            "choices": [
                {
                    "jpText": "明けましておめでとう！",
                    "enText": "Happy New Year!",
                    "nextBlock": 2
                }
            ],
            "animData": [
                {
                    "origLen": 139,
                    "pathId": -1107302189450017370
                },
