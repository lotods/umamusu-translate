            ],
            "pathId": -6191029637175291332,
            "blockIdx": 7
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "…………は？",
            "enText": "What about ………?",
            "nextBlock": 9,
            "origClipLength": 52,
            "voiceIdx": 26,
            "animData": [
                {
                    "origLen": 121,
                    "pathId": 2130340250001814070
                },
                {
                    "origLen": 139,
