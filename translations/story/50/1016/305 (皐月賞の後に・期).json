            "pathId": -1247116550651265440,
            "blockIdx": 23
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "ハッ、何を言う。",
            "enText": "Ha, what do you say.",
            "nextBlock": 25,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -1151284248742333434
                },
                {
                    "origLen": 66,
                    "pathId": -6075154741183772727
                }
