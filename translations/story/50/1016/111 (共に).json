            "blockIdx": 10
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……トレーナー。",
            "enText": "…… trainer.",
            "nextBlock": 12,
            "origClipLength": 55,
            "voiceIdx": 57,
            "choices": [
                {
                    "jpText": "マスコミ対応は任せて",
