            "blockIdx": 10
        },
        {
            "jpName": "取材陣",
            "enName": "",
            "jpText": "わっ！？",
            "enText": "Wow!",
            "nextBlock": 12,
            "origClipLength": 50,
            "choices": [
                {
                    "jpText": "す、すみません……",
