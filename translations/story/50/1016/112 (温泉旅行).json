            "pathId": 5498171715441273124,
            "blockIdx": 16
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……と。",
            "enText": "With ……",
            "nextBlock": 18,
            "origClipLength": 50,
            "choices": [
                {
                    "jpText": "どうした？",
                    "enText": "What's up?",
                    "nextBlock": 18
                },
                {
                    "jpText": "どうしたの？",
                    "enText": "What's wrong?",
                    "nextBlock": 18
                }
            ],
            "animData": [
                {
                    "origLen": 30,
                    "pathId": 5986978093289923165
                },
                {
                    "origLen": 61,
                    "pathId": -5495017539677021832
                }
