            "pathId": -5470522806944424861,
            "blockIdx": 19
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……。",
            "enText": "……",
            "nextBlock": 21,
            "origClipLength": 48,
            "animData": [
