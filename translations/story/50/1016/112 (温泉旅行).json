            "pathId": -3775369147737899154,
            "blockIdx": 6
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "温泉に。",
            "enText": "Onsen.",
            "nextBlock": 8,
            "origClipLength": 50,
            "pathId": -7790015998517736769,
            "blockIdx": 7
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "そして、後日。",
            "enText": "And later.",
            "nextBlock": 9,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 94,
                    "pathId": 3244442657509313562
