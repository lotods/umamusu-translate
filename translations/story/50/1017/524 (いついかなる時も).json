            "blockIdx": 2
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "――会長！",
            "enText": "―Chairman!",
            "nextBlock": 4,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 59,
                    "pathId": -7467165426844235728
