            "pathId": 7380485170161747775,
            "blockIdx": 8
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "トレーナー君――",
            "enText": "Ms. Trainer―",
            "nextBlock": 10,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 11,
