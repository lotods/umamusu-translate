            "blockIdx": 1
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "トレーナー君、話がある。",
            "enText": "Ms. Trainer, I need to talk to you.",
            "nextBlock": 3,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 24,
                    "pathId": 2064467519137050944
                },
                {
                    "origLen": 71,
