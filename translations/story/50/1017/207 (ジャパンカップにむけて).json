            "pathId": -737282404976595110,
            "blockIdx": 10
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "よろしい。",
            "enText": "Good.",
            "nextBlock": 12,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 22,
                    "pathId": 5025455199757651841
