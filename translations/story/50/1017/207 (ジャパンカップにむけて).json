            "blockIdx": 2
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Horsegirl A",
            "jpText": "はいっ、失礼しました……！",
            "enText": "Yes, excuse me. ……!",
            "nextBlock": 4,
            "origClipLength": 53,
            "choices": [
                {
                    "jpText": "あの子、今日一緒に走る子だな",
                    "enText": "That's the girl you're running with today.",
                    "nextBlock": 4
                },
                {
                    "jpText": "あの子、今日一緒に走る子だね",
                    "enText": "That's the girl you're running with today.",
                    "nextBlock": 4
                }
            ],
            "animData": [
                {
                    "origLen": 89,
