            "pathId": -1311691495741823514,
            "blockIdx": 5
        },
        {
            "jpName": "シンボリルドルフ",
            "enName": "Symboli Rudolf",
            "jpText": "入ってくれ。",
            "enText": "Come on in.",
            "nextBlock": 7,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 29,
                    "pathId": -7654198785126991958
                },
                {
                    "origLen": 59,
