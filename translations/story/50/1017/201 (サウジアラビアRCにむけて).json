                },
                {
                    "origLen": 81,
                    "pathId": -6856261490425841286
                }
            ],
            "pathId": 8840786590185118868,
            "blockIdx": 4
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（コンコン……）",
            "enText": "(Con-Con: ……)",
            "nextBlock": 6,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 21,
                    "pathId": 7756396035645856803
                },
