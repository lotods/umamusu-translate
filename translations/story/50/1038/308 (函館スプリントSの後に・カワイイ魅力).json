        {
            "jpName": "カレンチャン",
            "enName": "Curren Chan",
            "jpText": "そう……そうなのっ！",
            "enText": "So…… it is!",
            "nextBlock": 18,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 50,
                    "pathId": 7389991613117251094
                },
