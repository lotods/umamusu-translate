        },
        {
            "jpName": "カレンチャン",
            "enName": "Curren Chan",
            "jpText": "だってせっかく走っても、\r\nたくさんの人に見てもらえないんじゃ\nもったいないもん！",
            "enText": "Because it would be a waste to run and not \nhave lots of people see it!",
            "nextBlock": 16,
            "origClipLength": 98,
            "animData": [
                {
                    "origLen": 104,
                    "pathId": -531061763218778449
                },
                {
                    "origLen": 109,
                    "pathId": -3171796051682785223
