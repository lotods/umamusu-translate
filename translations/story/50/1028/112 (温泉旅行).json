            "blockIdx": 6
        },
        {
            "jpName": "ヒシアケボノ",
            "enName": "Hishi Akebono",
            "jpText": "たのも～～～っ！",
            "enText": "TOMO～!!!",
            "nextBlock": 8,
            "origClipLength": 55,
            "choices": [
                {
                    "jpText": "どうした！？",
                    "enText": "What's up!",
                    "nextBlock": 8
                },
                {
                    "jpText": "どうしたの！？",
                    "enText": "What's wrong with you!",
                    "nextBlock": 8
