            "blockIdx": 22
        },
        {
            "jpName": "ヒシアケボノ",
            "enName": "Hishi Akebono",
            "jpText": "全部、ボーノ……っ！",
            "enText": "All of them, bono ……!",
            "nextBlock": 24,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 161,
                    "pathId": -8346995141128762117
                },
