            "blockIdx": 27
        },
        {
            "jpName": "ヒシアケボノ",
            "enName": "Hishi Akebono",
            "jpText": "あはっ。どすこい、ふぁいとー☆",
            "enText": "Haha. Dosukoy, woohoo*!",
            "nextBlock": 29,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 47,
                    "pathId": -6181384876829804375
                },
                {
                    "origLen": 75,
                    "pathId": 5396318018164172680
                },
