        },
        {
            "jpName": "ヒシアケボノ",
            "enName": "Hishi Akebono",
            "jpText": "ちゃんこ鍋になってるー！？",
            "enText": "It's a chanko-nabe!",
            "nextBlock": 9,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 14,
                    "pathId": -6099799441618356259
                },
                {
                    "origLen": 14,
                    "pathId": -8220686918989904850
                }
