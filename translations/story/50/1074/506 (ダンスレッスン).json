        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（もさもさもさ～）\r\n（もさもさもさ～）\n（もさもさもさ～）",
            "enText": "(Mossy, mossy～) (Mossy, mossy～) (Mossy, \nmossy～) (Mossy, mossy～)",
            "nextBlock": 24,
            "origClipLength": 84,
            "animData": [
                {
                    "origLen": 14,
                    "pathId": 7303085275228813578
                },
                {
                    "origLen": 14,
