        {
            "jpName": "幼いブライト",
            "enName": "",
            "jpText": "ううう～。",
            "enText": "Uhhhhh～.",
            "nextBlock": 15,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 93,
                    "pathId": -5886166626003095227
                },
                {
                    "origLen": 93,
