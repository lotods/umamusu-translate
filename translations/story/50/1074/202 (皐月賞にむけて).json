            "pathId": -6042349271907185789,
            "blockIdx": 11
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "うわぁぁああああああああああッ！！",
            "enText": "WOW!!!!!!!",
            "nextBlock": 13,
            "origClipLength": 67,
            "animData": [
                {
                    "origLen": 88,
                    "pathId": 208952472686606113
                },
                {
                    "origLen": 96,
                    "pathId": -7222379019864399774
