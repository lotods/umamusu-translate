            "voiceIdx": -1,
            "choices": [
                {
                    "jpText": "そうではない",
                    "enText": "No, it isn't.",
                    "nextBlock": 4
                },
                {
                    "jpText": "そうじゃない",
                    "enText": "No, it isn't.",
