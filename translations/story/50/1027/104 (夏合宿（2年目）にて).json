                }
            ],
            "pathId": 6071093220087203559,
            "blockIdx": 8
        },
        {
            "jpName": "海水浴客Ｃ",
            "enName": "",
            "jpText": "ゲームしましょ、ゲーム！",
            "enText": "Let's play games. Games!",
            "nextBlock": 10,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 130,
                    "pathId": -3312766834740769192
                },
                {
                    "origLen": 140,
