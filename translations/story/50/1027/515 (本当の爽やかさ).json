            "pathId": -4509423488863936468,
            "blockIdx": 15
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "はぁ、はぁ……。",
            "enText": "Ha, ha. ……",
            "nextBlock": 17,
            "origClipLength": 81,
            "animData": [
                {
                    "origLen": 19,
