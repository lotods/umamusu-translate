            "pathId": -1828863912629272532,
            "blockIdx": 18
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "ふぅ～……。",
            "enText": "Hmmm… ……",
            "nextBlock": 20,
            "origClipLength": 52,
            "choices": [
                {
                    "jpText": "お疲れ様",
                    "enText": "that's enough for today",
                    "nextBlock": 20
                }
            ],
            "animData": [
                {
                    "origLen": 59,
                    "pathId": 2921733523637390152
