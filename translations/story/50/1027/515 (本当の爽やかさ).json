            "pathId": -6103780638678605241,
            "blockIdx": 14
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "は、はいっ！　お願いします！",
            "enText": "Ha, yes! Please!",
            "nextBlock": 16,
            "origClipLength": 63,
            "animData": [
                {
                    "origLen": 70,
                    "pathId": -3576494684056959047
                },
                {
                    "origLen": 58,
                    "pathId": 8651617707562517797
