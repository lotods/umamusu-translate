            "pathId": -5490165348089679020,
            "blockIdx": 23
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "あんな風な――スターになりたかった！！",
            "enText": "I wanted to be like that―a star!",
            "nextBlock": 25,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 52,
                    "pathId": 7820763043175367101
                },
                {
