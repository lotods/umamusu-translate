            "pathId": -8694652863935079456,
            "blockIdx": 24
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "は！？",
            "enText": "Ha!",
            "nextBlock": 26,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "それが答えかな",
                    "enText": "I guess that answers that question.",
                    "nextBlock": 26
                },
                {
                    "jpText": "それが答えみたいだね",
                    "enText": "That seems to be the answer.",
                    "nextBlock": 26
                }
            ],
            "animData": [
                {
                    "origLen": 60,
                    "pathId": -7488382734959674188
                },
                {
                    "origLen": 60,
                    "pathId": 1368196551846125317
                }
