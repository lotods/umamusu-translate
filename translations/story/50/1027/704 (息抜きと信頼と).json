            "pathId": 6910443257273515164,
            "blockIdx": 6
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "……えへへ。",
            "enText": "…… ehehe.",
            "nextBlock": 8,
            "origClipLength": 52,
            "choices": [
                {
                    "jpText": "どうかしたのか？",
                    "enText": "What's wrong?",
                    "nextBlock": 8
                },
                {
                    "jpText": "どうかしたの？",
                    "enText": "What's wrong?",
                    "nextBlock": 8
                }
