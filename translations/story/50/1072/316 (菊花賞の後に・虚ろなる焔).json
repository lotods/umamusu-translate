        },
        {
            "jpName": "ヤエノムテキ",
            "enName": "Yaeno Muteki",
            "jpText": "ぐ……う、ぅぅうッ…………！",
            "enText": "Gu…… ugh, ugh ………!",
            "nextBlock": 6,
            "origClipLength": 63,
            "choices": [
                {
                    "jpText": "（……ヤエノ……）",
                    "enText": "(…… Yaeno ……)",
