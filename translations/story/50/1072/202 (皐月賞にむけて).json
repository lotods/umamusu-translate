                },
                {
                    "origLen": 91,
                    "pathId": -6503848769125611954
                }
            ],
            "pathId": -8156173438177541611,
            "blockIdx": 31
        },
        {
            "jpName": "ヤエノムテキ",
            "enName": "Yaeno Muteki",
            "jpText": "――破ァッ！",
            "enText": "―Breach!",
            "nextBlock": 33,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 50,
