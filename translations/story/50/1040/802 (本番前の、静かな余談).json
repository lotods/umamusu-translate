            "blockIdx": 11
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "コイツのしつこさ、舐めてたわ。",
            "enText": "I licked this guy's persistence.",
            "nextBlock": 13,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 1228410476911978182
                },
                {
                    "origLen": 74,
                    "pathId": -3263764135763171929
