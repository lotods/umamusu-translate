            "pathId": -7989421279540851214,
            "blockIdx": 28
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "んぐっ……くっそ……！",
            "enText": "Ugh… damn…!",
            "nextBlock": 30,
            "origClipLength": 59,
            "pathId": 3775019268411884912,
            "blockIdx": 29
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "――あ～～～～！\r\nもうっっっっ！",
            "enText": "―ah ～～～～! Mooooooooo!!!",
