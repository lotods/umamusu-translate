            "blockIdx": 10
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "……。",
            "enText": "…",
            "nextBlock": 12,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "何から見ようか！",
                    "enText": "Where to start looking!",
                    "nextBlock": 12
