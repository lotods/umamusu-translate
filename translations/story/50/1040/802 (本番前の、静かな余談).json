            "pathId": 2478182364578843942,
            "blockIdx": 23
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "……。\r\n……もっかいだけいい？",
            "enText": "… Can I just have one more?",
            "nextBlock": 25,
            "origClipLength": 65,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 110660510928270501
                },
                {
                    "origLen": 60,
                    "pathId": 5841481202913967146
                },
                {
                    "origLen": 76,
                    "pathId": 5583220199635641917
                }
