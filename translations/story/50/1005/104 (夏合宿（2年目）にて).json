        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ダダダダッ！）",
            "enText": "(Dada dada dada!)",
            "nextBlock": 15,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 66,
