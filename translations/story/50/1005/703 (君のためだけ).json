            "blockIdx": 22
        },
        {
            "jpName": "ゾンビたち",
            "enName": "",
            "jpText": "ウオオオオ～～！",
            "enText": "Wooooooooo～!",
            "nextBlock": 24,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 67,
                    "pathId": -8224156006556239356
                },
                {
                    "origLen": 89,
                    "pathId": -922922961619780670
