            "nextBlock": 25,
            "origClipLength": 66,
            "choices": [
                {
                    "jpText": "大舞台の『天皇賞（春）』に挑もう！",
                    "enText": "Let's take on the big stage, \n\"Tennou-sho (Spring)\"!",
                    "nextBlock": 25
                }
