            "blockIdx": 27
        },
        {
            "jpName": "トウカイテイオー",
            "enName": "Tokai Teio",
            "jpText": "いたたたた……。",
            "enText": "Ita-ta-ta-ta. ……",
            "nextBlock": 29,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 109,
                    "pathId": 7405707224592704819
                },
