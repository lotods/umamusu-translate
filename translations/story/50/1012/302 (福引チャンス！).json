            "blockIdx": 18
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "へ？",
            "enText": "Heh?",
            "nextBlock": 20,
            "origClipLength": 47,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 1948424239114876186
                },
