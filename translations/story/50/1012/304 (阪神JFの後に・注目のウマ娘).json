        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "アンタたち！\r\nなんだい、こんなとこまで！",
            "enText": "You guys! What are you doing here?",
            "nextBlock": 8,
            "origClipLength": 72,
            "voiceIdx": 26,
            "animData": [
                {
                    "origLen": 61,
                    "pathId": 5688175371642529944
