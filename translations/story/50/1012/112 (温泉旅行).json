            "pathId": 3154204651908778237,
            "blockIdx": 19
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "なんだか実家を思い出すよ。",
            "enText": "It kind of reminds me of my parents' house.",
            "nextBlock": 21,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 93,
                    "pathId": 4106909576902455657
                },
                {
                    "origLen": 72,
                    "pathId": -7145339426495119880
                }
