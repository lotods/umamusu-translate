            ],
            "pathId": -3793499256157164509,
            "blockIdx": 2
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "ここまでいい香りがしてくる。\r\nへへっ。楽しみだ！",
            "enText": "It smells so good up here. Hehe. Looking \nforward to it!",
            "nextBlock": 4,
            "origClipLength": 77,
            "animData": [
                {
                    "origLen": 148,
                    "pathId": -4312530709582326384
