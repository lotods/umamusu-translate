            "blockIdx": 3
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……はぁあ――！",
            "enText": "…… haaaa―!",
            "nextBlock": 5,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 58,
                    "pathId": 1349892183858981145
                },
                {
                    "origLen": 58,
