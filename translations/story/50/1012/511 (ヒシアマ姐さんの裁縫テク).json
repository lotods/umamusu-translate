        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "よーし、ちょちょいのちょいだ！",
            "enText": "All right, a little bit of \nthis, a little bit of that!",
            "nextBlock": 22,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 16,
                    "pathId": 2192754264442834035
                },
                {
                    "origLen": 16,
