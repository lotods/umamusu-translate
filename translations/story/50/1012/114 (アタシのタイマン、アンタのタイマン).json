            "pathId": -7707874680508222636,
            "blockIdx": 18
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "……なるほど……ふん……へぇ～！\r\nこりゃあ……あはは！",
            "enText": "I see…… I see…… Hmmm…… Heh! Holy…… haha!",
            "nextBlock": 20,
            "origClipLength": 81,
            "animData": [
                {
                    "origLen": 92,
                    "pathId": 4253291567670825712
                },
                {
                    "origLen": 92,
                    "pathId": 8839755748389875616
