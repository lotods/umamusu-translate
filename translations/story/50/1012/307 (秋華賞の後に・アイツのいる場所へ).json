            "blockIdx": 8
        },
        {
            "jpName": "インタビューアー",
            "enName": "",
            "jpText": "と、言いますと？",
            "enText": "And you say?",
            "nextBlock": 10,
            "origClipLength": 55,
            "voiceIdx": -1,
            "animData": [
                {
                    "origLen": 66,
                    "pathId": -643644056869425901
