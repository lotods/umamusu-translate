            "blockIdx": 6
        },
        {
            "jpName": "ビコーペガサス",
            "enName": "Biko Pegasus",
            "jpText": "ペガサスマッハストラーーーイクッ！！",
            "enText": "Pegasus Machstlerooikkk!!!!",
            "nextBlock": 8,
            "origClipLength": 98,
            "animData": [
                {
                    "origLen": 58,
