            "pathId": 1324460594558167580,
            "blockIdx": 5
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "いち！　さん！　に！　よん！　いちぃ！？",
            "enText": "One! Three! Two! Four! Oney!",
            "nextBlock": 7,
            "origClipLength": 71,
            "choices": [
                {
                    "jpText": "ど、どこに行くんだ？",
                    "enText": "Which, where are you going?",
                    "nextBlock": 7
                },
                {
                    "jpText": "ど、どこに行くの？",
                    "enText": "Which, where are you going?",
                    "nextBlock": 7
                }
