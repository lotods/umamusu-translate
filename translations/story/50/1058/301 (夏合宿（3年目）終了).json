            "enText": "…………",
            "nextBlock": 12,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "君は……！？",
                    "enText": "You are ……!",
                    "nextBlock": 12
                },
                {
                    "jpText": "貴方は……！？",
                    "enText": "Are you ……!",
