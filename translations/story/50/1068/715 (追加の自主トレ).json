        },
        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "ありがとうございます！！",
            "enText": "Thank you!",
            "nextBlock": 9,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 23,
                    "pathId": 8213359846397360542
                },
                {
                    "origLen": 71,
                    "pathId": -2277789190637574964
