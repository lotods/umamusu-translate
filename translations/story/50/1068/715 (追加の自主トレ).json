        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "あの、トレーナーさんっ！",
            "enText": "Um, trainer!",
            "nextBlock": 3,
            "origClipLength": 60,
            "animData": [
                {
