            "pathId": 7228195172285697193,
            "blockIdx": 21
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "――花でも摘みに行くか？",
            "enText": "―I'm going to go pick some flowers.",
            "nextBlock": 23,
            "origClipLength": 60,
            "voiceIdx": -1,
            "animData": [
                {
                    "origLen": 62,
                    "pathId": 2026440947410009498
