            "blockIdx": 2
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "セコンドは、この私めにお任せを！",
            "enText": "Leave it to me to be the second!",
            "nextBlock": 4,
            "origClipLength": 65,
            "animData": [
                {
                    "origLen": 93,
                    "pathId": -7567256510726351991
                },
