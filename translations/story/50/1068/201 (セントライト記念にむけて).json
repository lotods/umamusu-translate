        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "行きましょうトレーナーさん！\n本番は3000m。今日はまず2200mです！",
            "enText": "Let's go, trainer! The real test \nis 3,000 meters, but today we'll \nstart with 2,200 meters!",
            "nextBlock": 3,
            "origClipLength": 93,
            "choices": [
                {
                    "jpText": "ああ、行こう！",
                    "enText": "Yeah, let's go!",
                    "nextBlock": 3
                },
                {
                    "jpText": "うん、行こう！",
                    "enText": "Yeah, let's go!",
                    "nextBlock": 3
                }
            ],
