            "blockIdx": 6
        },
        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "次は勝ってみせますっ！",
            "enText": "I will win next time!",
            "nextBlock": 8,
            "origClipLength": 59,
            "voiceIdx": 71,
            "animData": [
                {
                    "origLen": 42,
                    "pathId": -7075477225331072840
