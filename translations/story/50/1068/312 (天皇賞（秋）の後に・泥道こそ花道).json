            "blockIdx": 17
        },
        {
            "jpName": "キタサンブラック",
            "enName": "Kitasan Black",
            "jpText": "トレーナーさん、ありがとうございました！",
            "enText": "Thank you, trainer!",
            "nextBlock": 19,
            "origClipLength": 87,
            "voiceIdx": 56,
            "animData": [
                {
                    "origLen": 131,
                    "pathId": -661245920063873797
                },
