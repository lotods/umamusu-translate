    "title": "新年の抱負",
    "text": [
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "トレーナーさん、\r\nあけましておめでとうございます～♪",
            "enText": "Happy New Year, trainers!",
            "nextBlock": 2,
            "origClipLength": 103,
            "animData": [
                {
                    "origLen": 148,
                    "pathId": -6088382439811008678
                },
                {
                    "origLen": 143,
                    "pathId": 910382979686684060
