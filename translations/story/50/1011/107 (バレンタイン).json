                },
                {
                    "origLen": 70,
                    "pathId": 1498938313573332412
                }
            ],
            "pathId": 5097062192610607039,
            "blockIdx": 22
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "トレーナーさん……。",
            "enText": "Please contact the trainer at ……",
            "nextBlock": 24,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 36,
