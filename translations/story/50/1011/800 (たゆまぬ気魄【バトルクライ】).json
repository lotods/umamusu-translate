    "text": [
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "ある放課後。\n学園内を歩いてると……。",
            "enText": "One day after school. I'm walking around the \ncampus and I see ……",
            "nextBlock": 2,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 80,
                    "pathId": 3179205226078220646
                },
                {
                    "origLen": 80,
