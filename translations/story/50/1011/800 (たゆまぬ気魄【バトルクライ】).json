            "pathId": 2856658295337722637,
            "blockIdx": 1
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "あら、トレーナーさん。\nこんにちは。",
            "enText": "Oh, hey, trainer. Hello.",
            "nextBlock": 3,
            "origClipLength": 68,
            "choices": [
                {
                    "jpText": "その服どうしたんだ？",
                    "enText": "What happened to your clothes?",
                    "nextBlock": 3
                },
                {
                    "jpText": "その服どうしたの？",
                    "enText": "What happened to your clothes?",
                    "nextBlock": 3
                }
