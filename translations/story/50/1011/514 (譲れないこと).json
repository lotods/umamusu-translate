            "pathId": -2326119951902100060,
            "blockIdx": 15
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "ここへ、何をしに来たんですか？",
            "enText": "What are you doing here?",
            "nextBlock": 17,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 67,
                    "pathId": 3535090495890944442
                },
                {
                    "origLen": 66,
