            "pathId": 3893094321305433338,
            "blockIdx": 16
        },
        {
            "jpName": "男性A",
            "enName": "",
            "jpText": "えっ。",
            "enText": "Eh.",
            "nextBlock": 18,
            "origClipLength": 46,
            "animData": [
                {
                    "origLen": 57,
                    "pathId": 2440910414372453133
                },
                {
                    "origLen": 57,
                    "pathId": -3091837189500585092
                }
