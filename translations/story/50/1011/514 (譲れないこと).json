            "pathId": 473796418660219475,
            "blockIdx": 11
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "……ちょっと、行ってきますね。",
            "enText": "I'm going to go to…… for a bit.",
            "nextBlock": 13,
            "origClipLength": 54,
            "choices": [
                {
                    "jpText": "トラブルに巻き込まれたら危ない",
                    "enText": "It's not safe to get into trouble.",
                    "nextBlock": 13
                },
                {
                    "jpText": "行ってらっしゃい！",
                    "enText": "Off you go!",
                    "nextBlock": 24
                }
