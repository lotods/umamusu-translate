            "pathId": -4415844093109532770,
            "blockIdx": 23
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "ふふ、ありがとうございます～。",
            "enText": "Hmmm, thank you very much!",
            "nextBlock": 25,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 23,
                    "pathId": 8221311126965716323
                },
                {
                    "origLen": 65,
                    "pathId": 4031138939959096429
                }
