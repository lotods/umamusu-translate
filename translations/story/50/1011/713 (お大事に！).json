            "blockIdx": 1
        },
        {
            "jpName": "グラスワンダー",
            "enName": "Grass Wonder",
            "jpText": "……はぁ。\r\nすみません、ご迷惑をおかけして。",
            "enText": "…… Huh. Sorry for the inconvenience.",
            "nextBlock": 3,
            "origClipLength": 60,
            "choices": [
                {
                    "jpText": "俺の注意不足だ",
                    "enText": "I wasn't paying attention.",
                    "nextBlock": 4
                },
                {
                    "jpText": "今日は休んでまた頑張ろう！",
                    "enText": "Rest up today and let's go at it again!",
