            "nextBlock": 4,
            "origClipLength": 65,
            "choices": [
                {
                    "jpText": "今の君を見てもらおう！",
                    "enText": "Let them see you now!",
                    "nextBlock": 4
                },
                {
                    "jpText": "今の貴方を見てもらおう！",
                    "enText": "Let them see who you are now!",
                    "nextBlock": 4
