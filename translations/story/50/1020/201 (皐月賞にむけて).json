            "origClipLength": 91,
            "animData": [
                {
                    "origLen": 54,
                    "pathId": -1935364706725922608
                },
                {
                    "origLen": 102,
                    "pathId": -1965073341406147248
                }
            ],
            "pathId": -7782200006874433617,
            "blockIdx": 20
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "――狙った獲物を、\n誰かに譲る気もないんで。",
            "enText": "―I'm not going to give up my target to anyone.",
            "nextBlock": 22,
            "origClipLength": 73,
            "choices": [
                {
                    "jpText": "……！",
                    "enText": "……!",
                    "nextBlock": 22
