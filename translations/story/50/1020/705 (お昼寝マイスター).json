            "blockIdx": 1
        },
        {
            "jpName": "セイウンスカイ",
            "enName": "Seiun Sky",
            "jpText": "…………。\r\nzzz……。",
            "enText": "………… zzz ……",
            "nextBlock": 3,
            "origClipLength": 61,
            "choices": [
                {
                    "jpText": "起きて！",
                    "enText": "Get up!",
                    "nextBlock": 3
                }
