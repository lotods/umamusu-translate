    "title": "スプリンターズSにむけて",
    "text": [
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "……あの、トレーナーさん。",
            "enText": "…Excuse me, Trainer.",
            "nextBlock": 2,
            "origClipLength": 53,
            "voiceIdx": 54,
            "animData": [
                {
                    "origLen": 38,
                    "pathId": 1423020668205954962
