            "pathId": 7309919772334670414,
            "blockIdx": 23
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "ムムッ……ムムム……！",
            "enText": "Hm… Hmhmhmhm……!",
            "nextBlock": 25,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 127,
                    "pathId": 3305414805264236431
                },
                {
                    "origLen": 134,
