            "blockIdx": 20
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "……イイ！\r\nイイですよ、トレーナーさん！",
            "enText": "……Good! \nThat's good, Trainer-san!",
            "nextBlock": 22,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 50,
                    "pathId": -5856457181000149819
                },
                {
                    "origLen": 69,
                    "pathId": -6244870181964041412
                }
