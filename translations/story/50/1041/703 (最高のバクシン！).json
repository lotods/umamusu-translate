            "pathId": -7772576697428743366,
            "blockIdx": 9
        },
        {
            "jpName": "そばかす顔のウマ娘",
            "enName": "Freckled Horsegirl",
            "jpText": "（バクちゃん、みんな……本当にありがとう）",
            "enText": "(Baku-chan, everyone… Truly, thank you \nall so much.)",
            "nextBlock": 11,
            "origClipLength": 72,
            "voiceIdx": -1,
            "animData": [
                {
                    "origLen": 16,
                    "pathId": -208627151808470236
                },
                {
                    "origLen": 16,
                    "pathId": 3600344128907466856
                }
