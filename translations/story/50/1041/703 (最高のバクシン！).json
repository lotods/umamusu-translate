            "pathId": -6419769567184665316,
            "blockIdx": 18
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "ううっ……私の力不足です！",
            "enText": "Ugh… I'm not doing enough!",
            "nextBlock": 20,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 95,
                    "pathId": 448408034229163443
                },
                {
                    "origLen": 111,
                    "pathId": -2480906505972439058
                }
