            "animData": [
                {
                    "origLen": 91,
                    "pathId": -7816525035445823438
                },
                {
                    "origLen": 91,
                    "pathId": 8846337978549628013
                }
            ],
            "pathId": -7194799599762250055,
            "blockIdx": 27
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "ハイッ！",
            "enText": "Yes!",
            "nextBlock": 29,
            "origClipLength": 47,
            "voiceIdx": 48,
            "choices": [
                {
                    "jpText": "『有馬記念』を走るのに、1番大事なのは？",
                    "enText": "What's the most important thing about running the \nArima Kinen?",
                    "nextBlock": 29
                }
            ],
            "animData": [
                {
                    "origLen": 34,
                    "pathId": -2318459935644897429
                },
                {
                    "origLen": 58,
                    "pathId": -5414487482391273876
                }
