        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "……ガクッ。",
            "enText": "……Gulp.",
            "nextBlock": 5,
            "origClipLength": 52,
            "voiceIdx": 17,
            "choices": [
                {
                    "jpText": "失敗は悪いことじゃない",
                    "enText": "Failure isn't a bad thing.",
                    "nextBlock": 5
                },
                {
                    "jpText": "学級委員長、しっかり！",
                    "enText": "Class president, pull yourself together!",
