            "origClipLength": 67,
            "voiceIdx": -1,
            "choices": [
                {
                    "jpText": "頑張ってるな",
                    "enText": "You're working hard.",
                    "nextBlock": 7
                },
                {
                    "jpText": "頑張ってるね",
