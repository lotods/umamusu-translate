            "pathId": -1099241157451931036,
            "blockIdx": 18
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "にゃっ！？",
            "enText": "Meow!",
            "nextBlock": 20,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 29,
                    "pathId": 402321846731717017
