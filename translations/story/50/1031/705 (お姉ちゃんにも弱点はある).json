            "pathId": 3461543785889108285,
            "blockIdx": 25
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "……わああ～っ！？",
            "enText": "…… Wow!",
            "nextBlock": 27,
            "origClipLength": 56,
            "voiceIdx": 104,
            "choices": [
                {
                    "jpText": "お、落ち着い――うわっぷ！",
                    "enText": "Oh, calm down―whoops!",
                    "nextBlock": 27
