        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "お疲れ様でしたー……。",
            "enText": "Thanks for your time - ……",
            "nextBlock": 29,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 112,
                    "pathId": 8551380263474572541
