            "pathId": 8324417170689330733,
            "blockIdx": 8
        },
        {
            "jpName": "観客たち",
            "enName": "Crowd",
            "jpText": "アイネスーーっ！\nお姉ちゃーーーんっ！！",
            "enText": "Inezoooooo! Sis!!!!!!!",
            "nextBlock": 10,
            "origClipLength": 71,
            "animData": [
                {
                    "origLen": 72,
                    "pathId": -5659559427607410573
                },
                {
                    "origLen": 82,
