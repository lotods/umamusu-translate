            "blockIdx": 7
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "はぁー……ぜぇー……。\nはぁーー……っ。",
            "enText": "Huh…… ze…… Huh ……",
            "nextBlock": 9,
            "origClipLength": 77,
            "animData": [
                {
                    "origLen": 119,
                    "pathId": 8255908920240559911
                },
