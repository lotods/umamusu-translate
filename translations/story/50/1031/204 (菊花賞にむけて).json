            "pathId": -1878383848265284275,
            "blockIdx": 6
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "はっ……はっ……。\r\nくっ……。",
            "enText": "Ha…… ha…… Kuku……",
            "nextBlock": 8,
            "origClipLength": 65,
            "voiceIdx": 18,
            "animData": [
