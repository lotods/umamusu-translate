        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "えぇぇ～？",
            "enText": "Ehhhhh?",
            "nextBlock": 24,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "……やっぱり無理があるかな？",
                    "enText": "…… is it still too much to ask?",
                    "nextBlock": 24
                }
