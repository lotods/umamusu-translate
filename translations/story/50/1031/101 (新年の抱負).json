            "pathId": -2769583897791647356,
            "blockIdx": 24
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "そうだよ～。",
            "enText": "That's right～.",
            "nextBlock": 26,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 38,
                    "pathId": 3627671400449429284
                },
