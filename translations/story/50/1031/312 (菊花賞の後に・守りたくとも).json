            "pathId": -6895376712582655326,
            "blockIdx": 13
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "……！！　で、でも、あたしは……。",
            "enText": "……!!! And, but I'm ……",
            "nextBlock": 15,
            "origClipLength": 67,
            "animData": [
                {
                    "origLen": 39,
                    "pathId": 5336519444235958690
                },
                {
                    "origLen": 78,
                    "pathId": 1963578078690481109
                }
