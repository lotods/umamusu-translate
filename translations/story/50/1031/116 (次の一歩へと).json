            "pathId": -4915909404333284483,
            "blockIdx": 3
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "……うん、よかった。",
            "enText": "…… Yeah, good.",
            "nextBlock": 5,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 30,
                    "pathId": 2967443489523933938
                },
                {
                    "origLen": 69,
