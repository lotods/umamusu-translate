            "blockIdx": 4
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ヴー、ヴー！）",
            "enText": "(Voo, voo!)",
            "nextBlock": 6,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 84,
                    "pathId": -3122069861050455876
                },
                {
                    "origLen": 95,
                    "pathId": 1979911461657565757
