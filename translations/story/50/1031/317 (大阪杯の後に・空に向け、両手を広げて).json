        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "きもちいい～～……っ！！",
            "enText": "Feels good～～……!",
            "nextBlock": 13,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 72,
