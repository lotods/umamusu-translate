            "nextBlock": 37,
            "origClipLength": 98,
            "voiceIdx": -1,
            "choices": [
                {
                    "jpText": "（……でもいきなりGⅠが目標だなんて）",
                    "enText": "(I can't believe that even…… GI's are \nsuddenly the goal)",
                    "nextBlock": 37
                }
            ],
            "coloredText": [
                {
                    "jpText": "朝日杯FS",
                    "enText": "Asahi Cup FS"
