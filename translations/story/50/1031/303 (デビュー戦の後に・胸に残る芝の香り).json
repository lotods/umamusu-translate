            "blockIdx": 29
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "勝ちたいっ！！",
            "enText": "I want to win!",
            "nextBlock": 31,
            "origClipLength": 54,
            "voiceIdx": -1,
            "animData": [
