            "pathId": -7126199380521096461,
            "blockIdx": 17
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "……スーちゃん。",
            "enText": "…… Sue.",
            "nextBlock": 19,
            "origClipLength": 55,
