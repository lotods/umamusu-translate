            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "……ふぅ～…………。",
            "enText": "…… phew～…………",
            "nextBlock": -1,
            "origClipLength": 58,
            "voiceIdx": -1,
            "animData": [
                {
                    "origLen": 18,
                    "pathId": 6977800884858032262
