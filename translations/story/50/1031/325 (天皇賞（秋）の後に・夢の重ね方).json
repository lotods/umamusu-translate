        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "……！　ははっ……！！",
            "enText": "……! Haha ……!",
            "nextBlock": 11,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 2730072592044493123
                },
                {
                    "origLen": 39,
