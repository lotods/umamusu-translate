        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "ううっ……！",
            "enText": "Ugh ……!",
            "nextBlock": 6,
            "origClipLength": 52,
            "choices": [
                {
                    "jpText": "ひとまず診てもらおう！",
                    "enText": "Let's get her checked out for now!",
