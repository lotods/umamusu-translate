            "nextBlock": 6,
            "origClipLength": 71,
            "pathId": -3650272741874350355,
            "blockIdx": 5
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "どうしたんですか？",
            "enText": "What's wrong?",
