            "pathId": 2512666217161783219,
            "blockIdx": 6
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "これで私、もう大丈夫ですよね？",
            "enText": "Now I'm good to go, right?",
            "nextBlock": 8,
            "origClipLength": 64,
            "choices": [
                {
                    "jpText": "ああ！",
                    "enText": "Aah!",
                    "nextBlock": 8
                },
                {
                    "jpText": "うん！",
                    "enText": "Yeah!",
                    "nextBlock": 8
                }
