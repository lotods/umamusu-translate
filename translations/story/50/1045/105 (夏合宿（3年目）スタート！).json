            "blockIdx": 6
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "ん～？",
            "enText": "Hmm?",
            "nextBlock": 8,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "どうかした？",
                    "enText": "What's wrong?",
                    "nextBlock": 8
                },
                {
                    "jpText": "どうしたの？",
                    "enText": "What's wrong?",
                    "nextBlock": 8
                }
            ],
            "animData": [
                {
                    "origLen": 36,
