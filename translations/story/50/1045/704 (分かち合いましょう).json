            "pathId": -4270578505181102060,
            "blockIdx": 21
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "ええ、きっと。",
            "enText": "Yes, I'm sure.",
            "nextBlock": 23,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 79,
                    "pathId": 7374108160929582858
