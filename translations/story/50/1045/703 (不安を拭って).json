            "animData": [
                {
                    "origLen": 72,
                    "pathId": 7093964283548754370
                },
                {
                    "origLen": 99,
                    "pathId": 7766634460892475083
                }
            ],
            "pathId": 1684563082120579572,
            "blockIdx": 22
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "私は……あなたを不安にさせていますか？",
            "enText": "Am I making you uneasy ……?",
            "nextBlock": 24,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 66,
                    "pathId": -870601040402989161
                },
                {
                    "origLen": 80,
                    "pathId": 2039883102164374045
                }
            ],
            "pathId": 981435947758729715,
            "blockIdx": 23
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "倒れるまで無理して、勉強を続けて……。",
            "enText": "Push yourself until you collapse and continue \nstudying at ……",
            "nextBlock": 25,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 76,
                    "pathId": -1334105433710210203
                },
                {
                    "origLen": 80,
                    "pathId": -9046035757705270366
                }
            ],
            "pathId": 8311981917072836336,
            "blockIdx": 24
        },
        {
            "jpName": "スーパークリーク",
            "enName": "Super Creek",
            "jpText": "もし私のトレーナーでいることが、\r\nあなたを追い詰めているのなら……。",
            "enText": "If being my trainer is pushing you over the \nedge, please contact me at ……",
            "nextBlock": 26,
            "origClipLength": 90,
            "choices": [
                {
                    "jpText": "そんなことない！",
                    "enText": "That's not true!",
                    "nextBlock": 27
                },
                {
