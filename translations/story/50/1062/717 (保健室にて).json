            "pathId": 1575290337832593525,
            "blockIdx": 9
        },
        {
            "jpName": "マチカネタンホイザ",
            "enName": "Matikanetannhauser",
            "jpText": "わわっ……！　知らない料理ばっかり！？",
            "enText": "Wow! ……! All these dishes I don't know!",
            "nextBlock": 11,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 54,
                    "pathId": -7214193220926328535
                },
                {
                    "origLen": 80,
                    "pathId": 7448302008881564989
