            "blockIdx": 6
        },
        {
            "jpName": "マチカネタンホイザ",
            "enName": "Matikanetannhauser",
            "jpText": "あれあれ……！？\nなんでお父さんとお母さんが！？",
            "enText": "Hey hey hey ……! Why are \nyour father and mother!",
            "nextBlock": 8,
            "origClipLength": 76,
            "animData": [
                {
                    "origLen": 61,
                    "pathId": 3435512887221392236
                },
                {
                    "origLen": 87,
                    "pathId": 845975883674421288
                }
