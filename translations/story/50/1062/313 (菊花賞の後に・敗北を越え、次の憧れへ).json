            "blockIdx": 11
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "……！　はいっ！",
            "enText": "……! YES!!!",
            "nextBlock": 13,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 28,
                    "pathId": 4304774567099859318
                },
