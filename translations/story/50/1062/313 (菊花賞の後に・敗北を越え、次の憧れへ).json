        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "えっ……。",
            "enText": "What? ……",
            "nextBlock": 10,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 30,
