        {
            "jpName": "マチカネタンホイザ",
            "enName": "Matikanetannhauser",
            "jpText": "ほあ～～。\n届かなかった～……。",
            "enText": "Hoa～. I didn't get it～ ……",
            "nextBlock": 9,
            "origClipLength": 65,
            "voiceIdx": 60,
            "animData": [
                {
                    "origLen": 10,
                    "pathId": 8589634536213916628
