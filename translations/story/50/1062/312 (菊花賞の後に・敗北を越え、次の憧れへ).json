        {
            "jpName": "マチカネタンホイザ",
            "enName": "Matikanetannhauser",
            "jpText": "ほあ～～。\n届かなかった～……。",
            "enText": "Hoa～. I didn't get it～ ……",
            "nextBlock": 9,
            "origClipLength": 65,
            "animData": [
                {
                    "origLen": 10,
                    "pathId": -5153863293299574140
                },
                {
                    "origLen": 10,
