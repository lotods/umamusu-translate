            "blockIdx": 1
        },
        {
            "jpName": "マチカネタンホイザ",
            "enName": "Matikanetannhauser",
            "jpText": "うおお、うおおおお……っ！",
            "enText": "Woah, woah, woah ……!",
            "nextBlock": 3,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 80,
