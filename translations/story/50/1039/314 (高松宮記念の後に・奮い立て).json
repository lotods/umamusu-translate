            ],
            "pathId": -755860453136517709,
            "blockIdx": 28
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "……カワカミさん。\r\n………………。",
            "enText": "…… kawakami. ……………",
            "nextBlock": 30,
            "origClipLength": 68,
            "animData": [
                {
                    "origLen": 79,
                    "pathId": -3469140360504632451
                },
                {
                    "origLen": 79,
                    "pathId": 3188677680688397305
