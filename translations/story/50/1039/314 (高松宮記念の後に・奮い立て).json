            "blockIdx": 5
        },
        {
            "jpName": "カワカミプリンセス",
            "enName": "Kawakami Princess",
            "jpText": "……キング……さん。",
            "enText": "…… King ……",
            "nextBlock": 7,
            "origClipLength": 58,
            "animData": [
