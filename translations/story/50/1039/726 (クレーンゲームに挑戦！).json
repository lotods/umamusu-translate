            "nextBlock": 4,
            "origClipLength": 116,
            "choices": [
                {
                    "jpText": "欲しいのか？",
                    "enText": "You want it?",
                    "nextBlock": 4
                },
                {
                    "jpText": "欲しいの？",
                    "enText": "Do you want it?",
                    "nextBlock": 4
