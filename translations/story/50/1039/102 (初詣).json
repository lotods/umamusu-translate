            "pathId": -2616380131270750881,
            "blockIdx": 15
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ズシャァァァアアア！）",
            "enText": "(Zshaaaaaaaaa!)",
            "nextBlock": 17,
            "origClipLength": 60,
            "pathId": -6659086772343174247,
            "blockIdx": 16
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "盛大に足を滑らせた。",
            "enText": "I slipped up in a big way.",
            "nextBlock": 18,
            "origClipLength": 58,
            "choices": [
                {
                    "jpText": "大丈夫か！？",
                    "enText": "Are you okay!",
                    "nextBlock": 18
                },
                {
                    "jpText": "大丈夫！？",
                    "enText": "Great man!",
                    "nextBlock": 18
                }
