            "pathId": 830051948013728703,
            "blockIdx": 11
        },
        {
            "jpName": "カワカミプリンセス",
            "enName": "Kawakami Princess",
            "jpText": "そう……っ。",
            "enText": "So ……",
            "nextBlock": 13,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 31,
                    "pathId": 6785257435435976478
