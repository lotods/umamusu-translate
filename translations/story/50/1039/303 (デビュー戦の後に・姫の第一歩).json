            "blockIdx": 8
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "そしていつかは――",
            "enText": "And someday―",
            "nextBlock": 10,
            "origClipLength": 56,
            "choices": [
                {
                    "jpText": "最高のプリンセスになろう！",
                    "enText": "Be the best princess you can be!",
