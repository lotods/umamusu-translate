                {
                    "origLen": 134,
                    "pathId": 1628930615530174295
                }
            ],
            "pathId": -4924400703621890208,
            "blockIdx": 19
        },
        {
            "jpName": "カワカミプリンセス",
            "enName": "Kawakami Princess",
            "jpText": "早速、私も――",
            "enText": "I'll get right on it―",
            "nextBlock": 21,
            "origClipLength": 54,
            "animData": [
