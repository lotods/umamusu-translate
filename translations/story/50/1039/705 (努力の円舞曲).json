            "pathId": 50421132695410396,
            "blockIdx": 31
        },
        {
            "jpName": "女の子A",
            "enName": "Girl A",
            "jpText": "きゃあ～！　すごいすご～いっ！",
            "enText": "Oh my gosh! That's amazing!",
            "nextBlock": 33,
            "origClipLength": 64,
            "pathId": -6741716818362804175,
            "blockIdx": 32
        },
        {
            "jpName": "",
