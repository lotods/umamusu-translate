            "pathId": -6925973922822358935,
            "blockIdx": 13
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "今日はよろしくお願いします！\r\nカワカミさん！",
            "enText": "Nice to meet you today! Kawakami-san!",
            "nextBlock": 15,
            "origClipLength": 75,
            "voiceIdx": 144,
            "animData": [
                {
                    "origLen": 86,
                    "pathId": 1493498432587073542
                },
