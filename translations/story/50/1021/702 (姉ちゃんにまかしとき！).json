            "pathId": 1452320693442696858,
            "blockIdx": 15
        },
        {
            "jpName": "男の子A",
            "enName": "Boy A",
            "jpText": "そうかな？　えへへっ！",
            "enText": "Really? Heh heh heh!",
            "nextBlock": 17,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 139,
                    "pathId": 575903207813803618
                },
                {
                    "origLen": 71,
