        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "タマ……！　ああ……！",
            "enText": "Tama ……! Oh ……!",
            "nextBlock": 30,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 58,
