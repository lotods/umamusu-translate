            "pathId": -5871139962470697020,
            "blockIdx": 19
        },
        {
            "jpName": "タマモクロス",
            "enName": "Tamamo Cross",
            "jpText": "モグモグモグ……。",
            "enText": "Mogmogmog ……",
            "nextBlock": 21,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 55,
                    "pathId": -1994623334473663221
