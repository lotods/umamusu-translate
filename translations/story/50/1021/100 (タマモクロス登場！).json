            "blockIdx": 16
        },
        {
            "jpName": "タマモクロス",
            "enName": "Tamamo Cross",
            "jpText": "行っくでー！　最終直線！\n必殺『白い稲妻』！",
            "enText": "Let's go! Final straight line! \nSpecial \"White Lightning\"!",
            "nextBlock": 18,
            "origClipLength": 73,
            "coloredText": [
                {
                    "jpText": "白い稲妻",
                    "enText": "white lightning"
                }
            ],
            "animData": [
