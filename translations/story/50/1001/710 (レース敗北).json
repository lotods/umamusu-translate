            "pathId": -3529646704575976378,
            "blockIdx": 3
        },
        {
            "jpName": "スペシャルウィーク",
            "enName": "Special Week",
            "jpText": "トレーナーさん……ありがとうございます。",
            "enText": "Thank you trainer ……",
            "nextBlock": 5,
            "origClipLength": 71,
            "animData": [
                {
                    "origLen": 69,
                    "pathId": -5083062384039767158
                },
                {
                    "origLen": 82,
                    "pathId": 3737444557970792784
