            "origClipLength": 98,
            "choices": [
                {
                    "jpText": "遊園地とか？",
                    "enText": "Maybe an amusement park?",
                    "nextBlock": 5
                },
                {
                    "jpText": "山登りとか？",
                    "enText": "Like mountain climbing?",
