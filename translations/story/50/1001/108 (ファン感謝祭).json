            "origClipLength": 72,
            "choices": [
                {
                    "jpText": "今度は君の番だな",
                    "enText": "Now it's your turn.",
                    "nextBlock": 14
                },
                {
                    "jpText": "今度は貴方の番だね",
                    "enText": "Now it's your turn.",
