            "nextBlock": 9,
            "origClipLength": 62,
            "choices": [
                {
                    "jpText": "ああ！",
                    "enText": "Aah!",
                    "nextBlock": 9
                },
                {
                    "jpText": "うん！",
                    "enText": "Yeah!",
