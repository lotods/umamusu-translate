            "pathId": 5511578672678408743,
            "blockIdx": 22
        },
        {
            "jpName": "スペシャルウィーク",
            "enName": "Special Week",
            "jpText": "……ぱくっ。",
            "enText": "…… Paku.",
            "nextBlock": 24,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 61,
                    "pathId": 1926712586974391326
                },
                {
                    "origLen": 60,
