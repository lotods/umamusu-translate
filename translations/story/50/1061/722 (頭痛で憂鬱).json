        },
        {
            "jpName": "取り巻きーズ",
            "enName": "Followers",
            "jpText": "は～い！",
            "enText": "Yes!",
            "nextBlock": 10,
            "origClipLength": 47,
            "animData": [
                {
                    "origLen": 58,
