            "pathId": 7691295137625349588,
            "blockIdx": 11
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "……へえ？",
            "enText": "… Huh?",
            "nextBlock": 13,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "お母さんから？",
                    "enText": "From your mother?",
                    "nextBlock": 13
                }
            ],
            "animData": [
                {
                    "origLen": 60,
                    "pathId": -2103306319207750072
                },
                {
                    "origLen": 59,
                    "pathId": 9071845447642689866
