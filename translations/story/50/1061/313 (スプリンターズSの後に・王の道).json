            "pathId": -611140676698545641,
            "blockIdx": 13
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ピッ……）",
            "enText": "(Pic…)",
            "nextBlock": 15,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -2408122113470085647
                },
                {
                    "origLen": 60,
                    "pathId": 9061385753608829380
