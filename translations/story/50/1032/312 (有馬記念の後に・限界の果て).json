            "pathId": 7497483281934128919,
            "blockIdx": 22
        },
        {
            "jpName": "アグネスタキオン",
            "enName": "Agnes Tachyon",
            "jpText": "ハハハハハ！\nハーハッハッハッハ！！",
            "enText": "Ha-ha-ha-ha! Ha-ha-ha-ha!",
            "nextBlock": 24,
            "origClipLength": 56,
            "voiceIdx": 1,
            "choices": [
                {
                    "jpText": "！？",
                    "enText": "What?",
                    "nextBlock": 24
                }
