        {
            "jpName": "アグネスデジタル",
            "enName": "Agnes Digital",
            "jpText": "エンダアアアアアアアアアアアア！！！！！",
            "enText": "Endaaaaaaaaa！！！！！",
            "nextBlock": 24,
            "origClipLength": 71,
            "animData": [
