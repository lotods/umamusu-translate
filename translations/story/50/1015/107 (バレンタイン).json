            "animData": [
                {
                    "origLen": 53,
                    "pathId": -672288123249053828
                },
                {
                    "origLen": 105,
                    "pathId": 151374590359206030
                }
            ],
            "pathId": 1646347782079394602,
            "blockIdx": 17
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "TM Opera O",
            "jpText": "よかった、よかった。\r\nあっはっは……。はは……。",
            "enText": "Good, good. Haha…… Ha ha ……",
            "nextBlock": 19,
            "origClipLength": 77,
            "animData": [
                {
                    "origLen": 38,
                    "pathId": -2079102730795512692
                },
                {
                    "origLen": 88,
                    "pathId": 330483361884466465
