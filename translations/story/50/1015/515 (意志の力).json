            "blockIdx": 29
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "TM Opera O",
            "jpText": "はーっはっはっはっ！\nようやくボクを認めたようだね、天よ！",
            "enText": "Ha-ha-ha! You finally admit me, heavens!",
            "nextBlock": 31,
            "origClipLength": 85,
            "animData": [
                {
                    "origLen": 92,
                    "pathId": -8616460435910863842
                },
                {
                    "origLen": 81,
                    "pathId": 5491747829835613836
                },
