            "pathId": -216645108528263681,
            "blockIdx": 18
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "TM Opera O",
            "jpText": "テイッ、エムッ♪\r\nオーぺーラー……オォォォォォォォォォ～♪",
            "enText": "",
            "nextBlock": 20,
            "origClipLength": 84,
            "animData": [
                {
                    "origLen": 73,
                    "pathId": 2966594063557781397
                },
                {
                    "origLen": 95,
                    "pathId": 7896411547186278894
