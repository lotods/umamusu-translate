        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "チッ……。",
            "enText": "Tch. ……",
            "nextBlock": 14,
            "origClipLength": 59,
            "choices": [
                {
                    "jpText": "（……何ができるのだろうか？）",
                    "enText": "(…… what can we do?)",
                    "nextBlock": 14
                }
