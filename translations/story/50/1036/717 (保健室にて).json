            "blockIdx": 12
        },
        {
            "jpName": "謎の男",
            "enName": "",
            "jpText": "何ィ！？",
            "enText": "What?",
            "nextBlock": 14,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 61,
                    "pathId": 8990249621154620561
