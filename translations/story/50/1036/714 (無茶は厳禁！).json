            "blockIdx": 5
        },
        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "チッ……。",
            "enText": "Tch. ……",
            "nextBlock": 7,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 9,
