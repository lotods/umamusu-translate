            "blockIdx": 3
        },
        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "……？\nやけに気合い入ってンな。",
            "enText": "……? You seem awfully enthusiastic.",
            "nextBlock": 5,
            "origClipLength": 65,
            "animData": [
                {
                    "origLen": 37,
                    "pathId": 8902863320043925556
                },
                {
                    "origLen": 76,
                    "pathId": 9176558914348698364
                }
