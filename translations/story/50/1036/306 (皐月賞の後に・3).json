            "pathId": 2736914661357878748,
            "blockIdx": 4
        },
        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "チッ……。",
            "enText": "Tch. ……",
            "nextBlock": 6,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 58,
                    "pathId": 4323265633368803004
                },
                {
                    "origLen": 58,
                    "pathId": -7810419939204769615
