        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "黙秘。",
            "enText": "Silent.",
            "nextBlock": 11,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "ついていけばわかるかな",
