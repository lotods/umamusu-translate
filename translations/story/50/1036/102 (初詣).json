            "pathId": -7284487839891899157,
            "blockIdx": 20
        },
        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "……あァ、次だ。",
            "enText": "…… Oh, next.",
            "nextBlock": 22,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 103,
                    "pathId": -5726694327445803612
                },
                {
                    "origLen": 108,
                    "pathId": -5422567143871063156
                },
                {
                    "origLen": 66,
                    "pathId": -231250769256283601
                }
