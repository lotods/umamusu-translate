            "pathId": -8017086770015242840,
            "blockIdx": 21
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（～……♪）",
            "enText": "(～……♪)",
            "nextBlock": 23,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -8867017857044269952
                },
                {
                    "origLen": 20,
