            "pathId": 8052715220951197000,
            "blockIdx": 6
        },
        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "……ウゼ。",
            "enText": "…… uze.",
            "nextBlock": 8,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 94,
                    "pathId": 1511259059678633385
