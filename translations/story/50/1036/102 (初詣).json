            "blockIdx": 3
        },
        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "どーも。",
            "enText": "Hi, how are you?",
            "nextBlock": 5,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 76,
                    "pathId": -1178554071357935757
                },
                {
                    "origLen": 61,
                    "pathId": 6648457985865686993
                }
