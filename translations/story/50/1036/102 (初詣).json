        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "言うと思った。",
            "enText": "I thought I would say.",
            "nextBlock": 12,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 53,
                    "pathId": -8006916074125973489
                },
                {
                    "origLen": 65,
