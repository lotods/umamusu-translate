            "pathId": -3271463644081906806,
            "blockIdx": 12
        },
        {
            "jpName": "エアシャカール",
            "enName": "Air Shakur",
            "jpText": "散歩。",
            "enText": "Walking.",
            "nextBlock": 14,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "もう門限はとっくに過ぎてるぞ",
                    "enText": "It's long past curfew.",
                    "nextBlock": 14
                },
                {
                    "jpText": "もう門限はとっくに過ぎてるよ",
                    "enText": "It's long past curfew.",
