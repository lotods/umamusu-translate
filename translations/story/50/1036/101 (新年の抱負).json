            "origClipLength": 112,
            "choices": [
                {
                    "jpText": "取り引きをする",
                    "enText": "make a deal",
                    "nextBlock": 21
                },
                {
                    "jpText": "たくさんお願いする",
                    "enText": "I ask a lot.",
                    "nextBlock": 38
                },
                {
                    "jpText": "止めない",
                    "enText": "(after the -te form of a verb) won't do",
                    "nextBlock": 51
