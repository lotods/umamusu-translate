            "pathId": -1460407260661356170,
            "blockIdx": 17
        },
        {
            "jpName": "スマートファルコン",
            "enName": "Smart Falcon",
            "jpText": "えっ？　また？",
            "enText": "What? Again?",
            "nextBlock": 19,
            "origClipLength": 54,
            "voiceIdx": 26,
            "animData": [
                {
                    "origLen": 65,
                    "pathId": -7163784537796378161
                },
                {
                    "origLen": 65,
                    "pathId": -4807357159817552843
                },
                {
                    "origLen": 44,
                    "pathId": 3664822779326323804
