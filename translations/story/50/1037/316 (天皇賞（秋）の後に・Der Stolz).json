        {
            "jpName": "エイシンフラッシュ",
            "enName": "Eishin Flash",
            "jpText": "あっ……。\n……お父さん、お母さん。",
            "enText": "Oh…… …… father and mother.",
            "nextBlock": 14,
            "origClipLength": 68,
            "choices": [
                {
                    "jpText": "少し席を外します",
                    "enText": "I'll leave you alone for a moment.",
                    "nextBlock": 14
                }
            ],
            "animData": [
                {
                    "origLen": 41,
                    "pathId": 8181666816375486369
                },
