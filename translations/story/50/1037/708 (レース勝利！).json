            "pathId": -3644799382930965652,
            "blockIdx": 3
        },
        {
            "jpName": "エイシンフラッシュ",
            "enName": "Eishin Flash",
            "jpText": "はい。\r\nありがとうございます。",
            "enText": "Yes, I would like to know if you have any \nquestions. Thank you very much.",
            "nextBlock": 5,
            "origClipLength": 65,
            "animData": [
                {
                    "origLen": 50,
                    "pathId": 8028097996313834381
                },
                {
                    "origLen": 76,
                    "pathId": -2350317461508807883
                }
