            "pathId": 4264017811735745837,
            "blockIdx": 8
        },
        {
            "jpName": "インタビュアー",
            "enName": "Interviewer",
            "jpText": "『……はい？』",
            "enText": "…… yes?\"",
            "nextBlock": 10,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 65,
