    "title": "その時間は贈り物",
    "text": [
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "――もうすぐ、ホワイトデーだ。\r\nエイシンフラッシュに\n何かお返しをしたいのだが……。",
            "enText": "―I'd like to give something back to Eishin \nFlash. I'd like to give something back to \nEishin Flash. ……",
            "nextBlock": 2,
            "origClipLength": 101,
            "choices": [
                {
                    "jpText": "うう～ん……",
                    "enText": "Ummm… ……",
                    "nextBlock": 2
                }
            ],
            "animData": [
                {
                    "origLen": 130,
                    "pathId": -5054803329571274344
