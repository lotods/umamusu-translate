            "origClipLength": 68,
            "choices": [
                {
                    "jpText": "わかった",
                    "enText": "Okay, okay.",
                    "nextBlock": 13
                },
                {
                    "jpText": "実はそこにいる",
                    "enText": "Actually, she's there.",
                    "nextBlock": 25
