            "blockIdx": 2
        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "おおっ！？\r\n大丈夫ですか、トレーナーさん！",
            "enText": "Oh! Are you okay, trainer!",
            "nextBlock": 4,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 4001177468253357441
                },
                {
                    "origLen": 70,
