            "choices": [
                {
                    "jpText": "確か、そうだったな",
                    "enText": "As I recall, it did.",
                    "nextBlock": 20
                },
                {
                    "jpText": "確か、そうだったね",
                    "enText": "As I recall, you did.",
                    "nextBlock": 20
