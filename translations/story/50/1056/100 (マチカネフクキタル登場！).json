            "nextBlock": 5,
            "origClipLength": 73,
            "choices": [
                {
                    "jpText": "……本当だ",
                    "enText": "…… true.",
                    "nextBlock": 5
                },
                {
                    "jpText": "……本当だね",
                    "enText": "…… true.",
                    "nextBlock": 5
                }
