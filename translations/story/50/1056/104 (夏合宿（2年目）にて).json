            "origClipLength": 74,
            "animData": [
                {
                    "origLen": 85,
                    "pathId": 636533144178576079
                },
                {
                    "origLen": 85,
                    "pathId": 7520847746394362262
                }
            ],
            "pathId": -1720267656344787831,
            "blockIdx": 1
        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "トレーナーさん、『菊花賞』に向けて、\r\n今の私に足りないもの、なにかわかりますか？\nザックリで！",
            "enText": "Trainer, do you know what I'm missing \nright now for the Kikka-sho? I'll \ngive you a rough idea!",
            "nextBlock": 3,
            "origClipLength": 76,
            "choices": [
                {
                    "jpText": "……全部？",
                    "enText": "…… all?",
                    "nextBlock": 3
                }
