        {
            "jpName": "アドマイヤベガ",
            "enName": "Admire Vega",
            "jpText": "…………。",
            "enText": "…………",
            "nextBlock": 16,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "大丈夫そう？",
                    "enText": "Does it look okay?",
                    "nextBlock": 16
                }
