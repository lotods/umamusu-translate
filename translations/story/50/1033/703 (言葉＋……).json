            ],
            "pathId": -5890190875769898605,
            "blockIdx": 8
        },
        {
            "jpName": "アドマイヤベガ",
            "enName": "Admire Vega",
            "jpText": "ちょ、ちょっと！",
            "enText": "Hey, hey!",
            "nextBlock": 10,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 51,
                    "pathId": 1686440260738412464
                },
                {
                    "origLen": 66,
                    "pathId": -8145204664947476487
                }
