            "pathId": -1160095086802576257,
            "blockIdx": 6
        },
        {
            "jpName": "アドマイヤベガ",
            "enName": "Admire Vega",
            "jpText": "…………。",
            "enText": "…………",
            "nextBlock": 8,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 56,
                    "pathId": -3790156583143935297
                },
                {
                    "origLen": 36,
                    "pathId": -7798106081473083626
