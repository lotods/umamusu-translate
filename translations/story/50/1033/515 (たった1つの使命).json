            "pathId": -1056627256310004366,
            "blockIdx": 13
        },
        {
            "jpName": "アドマイヤベガ",
            "enName": "Admire Vega",
            "jpText": "…………ありがとうございます。",
            "enText": "Thank you …………",
            "nextBlock": 15,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 28,
                    "pathId": -1185461873841332470
                },
                {
                    "origLen": 75,
                    "pathId": 8840649738405756416
                }
