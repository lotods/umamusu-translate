            "pathId": -4441647811601741207,
            "blockIdx": 25
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "……なんだろう。",
            "enText": "I guess it's ……",
            "nextBlock": 27,
            "origClipLength": 50,
            "choices": [
                {
                    "jpText": "やっぱりわからないんじゃ……",
                    "enText": "I guess you still don't know. ……",
                    "nextBlock": 27
                }
            ],
            "animData": [
                {
                    "origLen": 19,
                    "pathId": -6448037549579848755
                },
                {
                    "origLen": 61,
                    "pathId": -1123878082250591537
                }
            ],
            "pathId": 1535858194487891495,
            "blockIdx": 26
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "あわわっ！？　ちがうちがうー！！\r\nちゃんと違いはあるんだもん～！！",
            "enText": "Aaah! No, no, no! There is a difference～!",
            "nextBlock": 28,
