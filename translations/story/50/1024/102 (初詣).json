            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 56,
                    "pathId": -647933864661232156
                },
                {
                    "origLen": 16,
                    "pathId": 9192647959446799054
                },
                {
                    "origLen": 62,
                    "pathId": 587237066845449016
                }
