            "blockIdx": 2
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "えへへっ♪　トレーナーちゃん、\r\nかたい顔はなしなし～♪",
            "enText": "Heh heh heh ♪ Trainer, don't \nmake a hard face～♪",
            "nextBlock": 4,
            "origClipLength": 63,
            "animData": [
                {
                    "origLen": 40,
                    "pathId": -1503528474802315120
                },
                {
                    "origLen": 74,
