            "pathId": -8312701410653315231,
            "blockIdx": 27
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "ほら～☆　マヤの言った通り～！",
            "enText": "See～☆ Just like Maya said～!",
            "nextBlock": 29,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 82,
                    "pathId": -5869499930266873615
                },
                {
                    "origLen": 94,
                    "pathId": 7326688959646369140
                }
            ],
            "pathId": -4844968121560766548,
            "blockIdx": 28
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "えへへっ、それじゃあ次いこ！　次っ！！",
            "enText": "Heh heh heh, next! Next!",
            "nextBlock": 30,
            "origClipLength": 57,
            "animData": [
                {
                    "origLen": 40,
                    "pathId": -4773950472683380140
                },
                {
                    "origLen": 68,
                    "pathId": -8406489234901910428
                }
            ],
            "pathId": -2090932383881771291,
            "blockIdx": 29
