            "animData": [
                {
                    "origLen": 40,
                    "pathId": -1467523852055793706
                },
                {
                    "origLen": 78,
                    "pathId": -2660605448591223865
                }
            ],
            "pathId": 2510334084435507124,
            "blockIdx": 17
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "Mayano Top Gun",
            "jpText": "はいはーい、わかってまーす☆\r\nそれにこーいうの、マヤ得意だから！",
