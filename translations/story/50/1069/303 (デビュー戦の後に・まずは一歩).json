            "pathId": -2878292466467313276,
            "blockIdx": 19
        },
        {
            "jpName": "サクラチヨノオー",
            "enName": "Sakura Chiyono O",
            "jpText": "……～～～～～っ！\nはいっ、私頑張りますっ！！",
            "enText": "……～～～～～! Yes, I'll do my best!",
            "nextBlock": 21,
            "origClipLength": 75,
            "animData": [
                {
                    "origLen": 58,
                    "pathId": -2264853358761250407
                },
                {
                    "origLen": 161,
                    "pathId": 3418607096120264351
