            "blockIdx": 20
        },
        {
            "jpName": "サクラチヨノオー",
            "enName": "Sakura Chiyono O",
            "jpText": "どんどん先へと流れて……\n様々なものが変わっていってしまう。",
            "enText": "It flows further and further ahead and…… many \nthings are changing.",
            "nextBlock": 22,
            "origClipLength": 84,
            "choices": [
                {
                    "jpText": "でも君の想いは変わっていないだろう？",
                    "enText": "But your feelings haven't changed, have they?",
                    "nextBlock": 22
                },
                {
                    "jpText": "でも貴方の想いは変わっていないよね？",
                    "enText": "But your feelings haven't changed, have they?",
                    "nextBlock": 22
                }
            ],
            "animData": [
                {
                    "origLen": 74,
                    "pathId": -7476577128946974077
                },
                {
                    "origLen": 95,
                    "pathId": 3835248457156009913
                }
