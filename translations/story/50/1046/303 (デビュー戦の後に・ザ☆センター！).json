            "animData": [
                {
                    "origLen": 76,
                    "pathId": 413036149315127171
                },
                {
                    "origLen": 97,
                    "pathId": 3553060632106621737
                }
            ],
            "pathId": -4924701226018732001,
            "blockIdx": 15
        },
        {
            "jpName": "スマートファルコン",
            "enName": "Smart Falcon",
            "jpText": "私、もっと大きなステージに立ちたい！\nトップウマドルになりたいっ！！",
            "enText": "I want to be on a bigger stage! \nI want to be a top madre!",
