        },
        {
            "jpName": "スマートファルコン",
            "enName": "Smart Falcon",
            "jpText": "そうっ！\nファル子、もうメラメラだよ～！",
            "enText": "Yes! Falco, I'm already melting down～!",
            "nextBlock": 5,
            "origClipLength": 71,
            "animData": [
                {
                    "origLen": 37,
                    "pathId": 8971063852710337794
                },
                {
                    "origLen": 82,
