            "pathId": 2532653211416867763,
            "blockIdx": 6
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "私、ついにデビューしたんですね！",
            "enText": "I see I finally made my debut!",
            "nextBlock": 8,
            "origClipLength": 65,
            "voiceIdx": 0,
            "choices": [
                {
                    "jpText": "そうだな",
                    "enText": "Right.",
                    "nextBlock": 8
                },
                {
                    "jpText": "そうだね",
                    "enText": "Yes, it is.",
                    "nextBlock": 8
                }
            ],
            "animData": [
                {
                    "origLen": 77,
                    "pathId": 4973126829425775762
                },
                {
                    "origLen": 76,
                    "pathId": -3615538424229228580
                }
