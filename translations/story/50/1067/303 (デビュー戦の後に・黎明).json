            "blockIdx": 6
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "私、ついにデビューしたんですね！",
            "enText": "I see I finally made my debut!",
            "nextBlock": 8,
            "origClipLength": 65,
            "choices": [
                {
                    "jpText": "そうだな",
                    "enText": "Right.",
                    "nextBlock": 8
                },
                {
                    "jpText": "そうだね",
                    "enText": "Yes, it is.",
                    "nextBlock": 8
                }
            ],
            "animData": [
                {
                    "origLen": 77,
                    "pathId": -4043265755092969821
                },
                {
                    "origLen": 76,
                    "pathId": 6580488874351310440
                }
