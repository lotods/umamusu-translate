            "pathId": 2072879996251221309,
            "blockIdx": 11
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "私も、やっと同じターフに……！",
            "enText": "I'm finally on the same turf ……!",
            "nextBlock": 13,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 45,
                    "pathId": 7523207329996632956
                },
                {
                    "origLen": 75,
                    "pathId": 2521023859618791717
                }
