            "pathId": -3791659779090051431,
            "blockIdx": 17
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "ジンクスには負けない！\nそう誓いましたのに！！",
            "enText": "I couldn't be jinxed! I swore I would!",
            "nextBlock": 19,
            "origClipLength": 75,
            "animData": [
                {
                    "origLen": 28,
                    "pathId": -247193257745803903
                },
                {
                    "origLen": 86,
                    "pathId": -8161808199423484774
