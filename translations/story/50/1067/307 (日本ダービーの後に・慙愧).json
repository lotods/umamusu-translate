            "origClipLength": 64,
            "voiceIdx": -1,
            "pathId": 1812406913854950128,
            "blockIdx": 3
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "（何もないッ！！）",
