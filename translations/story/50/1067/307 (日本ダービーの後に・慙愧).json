            "pathId": -4339186364967416764,
            "blockIdx": 20
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "あらゆる困難をものともしない強さを！\n万事に対応できる完璧な準備を！",
            "enText": "Strength against all odds! Perfect readiness \nto meet all challenges!",
            "nextBlock": 22,
            "origClipLength": 89,
            "voiceIdx": 11,
            "animData": [
                {
                    "origLen": 73,
                    "pathId": -6028815895539557734
                },
                {
                    "origLen": 100,
                    "pathId": 7812865244699644082
                }
