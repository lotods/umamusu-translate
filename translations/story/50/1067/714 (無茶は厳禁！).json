            "nextBlock": 16,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "大丈夫か！？",
                    "enText": "Are you okay!",
                    "nextBlock": 16
                },
                {
                    "jpText": "大丈夫！？",
                    "enText": "Great man!",
                    "nextBlock": 16
