            "blockIdx": 5
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "ふぅーー……。",
            "enText": "Whew. ……",
            "nextBlock": 7,
            "origClipLength": 54,
            "choices": [
                {
                    "jpText": "大丈夫か？",
                    "enText": "Are you okay?",
                    "nextBlock": 7
                },
                {
                    "jpText": "大丈夫？",
                    "enText": "Great man?",
                    "nextBlock": 7
                }
            ],
            "animData": [
                {
                    "origLen": 17,
                    "pathId": -6663890216421269373
                },
                {
                    "origLen": 17,
                    "pathId": 3799997844816785287
                },
