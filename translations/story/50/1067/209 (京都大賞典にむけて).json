            "pathId": 1621469489307701018,
            "blockIdx": 4
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "…………。",
            "enText": "…………",
            "nextBlock": 6,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 8524604122580395761
