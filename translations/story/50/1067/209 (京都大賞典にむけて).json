            "pathId": -4408182112795663771,
            "blockIdx": 4
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "…………。",
            "enText": "…………",
            "nextBlock": 6,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 2909125800225934991
