            "blockIdx": 34
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "そんな自分が許せないのです！！",
            "enText": "I can't allow myself to be like that!",
            "nextBlock": 36,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 55,
                    "pathId": 1613373842223094875
