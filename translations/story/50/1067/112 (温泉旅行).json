            "pathId": 5537550144725771426,
            "blockIdx": 12
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "入浴中……。",
            "enText": "Get in the bath at ……",
            "nextBlock": 14,
            "origClipLength": 250,
            "animData": [
                {
                    "origLen": 30,
                    "pathId": 2979379963753703782
                },
                {
                    "origLen": 30,
