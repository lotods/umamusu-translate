            "pathId": -4144440753563536442,
            "blockIdx": 10
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "……あー！！",
            "enText": "…… Ah!",
            "nextBlock": 12,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 19,
                    "pathId": -1482486283459051525
                },
                {
                    "origLen": 59,
                    "pathId": -6071773233270105911
                }
