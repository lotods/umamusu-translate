                },
                {
                    "origLen": 80,
                    "pathId": 5000823978872658687
                }
            ],
            "pathId": 1840350887306042382,
            "blockIdx": 13
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "それじゃあ、またね！\r\nトレーナー！！",
            "enText": "See you then! Trainer!",
            "nextBlock": 15,
            "origClipLength": 57,
            "animData": [
                {
                    "origLen": 19,
                    "pathId": -4192637957542192090
