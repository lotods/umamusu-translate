            "pathId": 5752665890299659167,
            "blockIdx": 9
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "…………。",
            "enText": "…………",
            "nextBlock": 11,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "何？",
                    "enText": "What?",
                    "nextBlock": 11
                }
            ],
            "animData": [
                {
                    "origLen": 118,
                    "pathId": -314744747587224364
                },
                {
                    "origLen": 59,
                    "pathId": 6456829380521266166
