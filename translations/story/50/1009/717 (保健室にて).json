            "blockIdx": 7
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "……あら？　ゆ、夢だったのかしら？",
            "enText": "…… Oh? Was I dreaming?",
            "nextBlock": 9,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 131,
                    "pathId": 206634085450371574
                },
                {
                    "origLen": 79,
                    "pathId": -4280957868543755805
                }
