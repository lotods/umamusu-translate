            "animData": [
                {
                    "origLen": 47,
                    "pathId": -8944097622447082103
                },
                {
                    "origLen": 78,
                    "pathId": 4888220625582289711
                }
            ],
            "pathId": -3667834826051739783,
            "blockIdx": 23
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "……アタシが欲しいのは労いの言葉じゃない。\r\nアタシを……アタシだけを賞賛する言葉よ。",
            "enText": "I don't want words of…… labor, I want words \nof praise for I. I want words of praise \nonly for…… I.",
            "nextBlock": 25,
            "origClipLength": 73,
            "choices": [
                {
                    "jpText": "俺の中では君が1番だよ",
                    "enText": "You're number one in my book.",
                    "nextBlock": 25
                },
                {
                    "jpText": "私の中では貴方が1番だよ",
                    "enText": "You're number one in my book.",
                    "nextBlock": 25
                }
