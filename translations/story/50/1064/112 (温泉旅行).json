            "pathId": 765483912501169194,
            "blockIdx": 3
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（ガラガラガラッ！）",
            "enText": "(Rattle rattle rattle!)",
            "nextBlock": 5,
            "origClipLength": 58,
            "pathId": -2159786967778526293,
            "blockIdx": 4
        },
        {
            "jpName": "メジロパーマー",
            "enName": "Mejiro Palmer",
            "jpText": "トレーナー！　温泉ターイム☆\nFuFu～！！！！！！！",
            "enText": "Trainer! Hot Springs Turnime☆ FuFu～！！！！！！！",
            "nextBlock": 6,
            "origClipLength": 80,
            "choices": [
                {
                    "jpText": "いきなり……何！？",
                    "enText": "Suddenly…… what!",
                    "nextBlock": 6
