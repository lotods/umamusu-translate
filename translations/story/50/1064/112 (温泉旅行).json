            "pathId": -928337373884331852,
            "blockIdx": 10
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "入浴中……。",
            "enText": "Get in the bath at ……",
            "nextBlock": 12,
            "origClipLength": 250,
            "pathId": -4995030885872562759,
            "blockIdx": 11
        },
        {
            "jpName": "メジロパーマー",
            "enName": "Mejiro Palmer",
            "jpText": "いや～、もうさ。最高のお湯だったよ！",
