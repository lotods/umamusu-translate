                },
                {
                    "origLen": 89,
                    "pathId": -3697819823920879567
                }
            ],
            "pathId": -6963667863579976434,
            "blockIdx": 31
        },
        {
            "jpName": "ナリタブライアン",
            "enName": "Narita Brian",
            "jpText": "……ああ。",
            "enText": "……Yeah.",
            "nextBlock": 33,
            "origClipLength": 56,
            "voiceIdx": 20,
            "animData": [
                {
                    "origLen": 116,
                    "pathId": 2223032865062081255
