            "pathId": 378276986688942466,
            "blockIdx": 7
        },
        {
            "jpName": "ヒシアマゾン",
            "enName": "Hishi Amazon",
            "jpText": "おっしゃあもう1本！！　いくよっ！！",
            "enText": "Alright, one more go!! Come on!!",
            "nextBlock": 9,
            "origClipLength": 76,
            "voiceIdx": 4,
            "animData": [
                {
                    "origLen": 58,
                    "pathId": 7438258553828580129
