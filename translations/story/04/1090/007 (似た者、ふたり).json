            "blockIdx": 24
        },
        {
            "jpName": "ヴィルシーナ",
            "enName": "Verxina",
            "jpText": "落ち着いて！　みんな！",
            "enText": "Calm down! Everyone!",
            "nextBlock": 26,
            "origClipLength": 71,
