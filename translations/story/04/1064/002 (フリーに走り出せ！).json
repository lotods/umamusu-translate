            "pathId": 5589295267384037583,
            "blockIdx": 23
        },
        {
            "jpName": "メジロパーマー",
            "enName": "Mejiro Palmer",
            "jpText": "……！！\nうん……よろしく！！",
            "enText": "……!!! Yeah…… regards!!!!!!!!",
            "nextBlock": 25,
            "origClipLength": 145,
            "animData": [
