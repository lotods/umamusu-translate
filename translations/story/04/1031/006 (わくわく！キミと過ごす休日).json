            "origClipLength": 244,
            "voiceIdx": 1,
            "animData": [
                {
                    "origLen": 130,
                    "pathId": 299483008719723377
                }
            ],
            "pathId": 3237470800285329487,
            "blockIdx": 3
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "……と思ったんだけど。",
