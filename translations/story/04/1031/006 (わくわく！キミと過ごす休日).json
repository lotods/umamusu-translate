            "voiceIdx": 2,
            "choices": [
                {
                    "jpText": "どうしたんだ？",
                    "enText": "What's going on?",
                    "nextBlock": 5
                },
                {
                    "jpText": "どうしたの？",
                    "enText": "What's wrong?",
                    "nextBlock": 5
                }
