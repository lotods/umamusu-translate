            "origClipLength": 91,
            "voiceIdx": -1,
            "choices": [
                {
                    "jpText": "今日は俺がエスコートしてもいい？",
                    "enText": "Can I be your escort today?",
                    "nextBlock": 8
                },
                {
                    "jpText": "今日は私がエスコートしてもいい？",
                    "enText": "Can I be your escort today?",
                    "nextBlock": 8
                }
