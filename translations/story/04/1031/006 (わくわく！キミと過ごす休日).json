            "nextBlock": 24,
            "origClipLength": 107,
            "choices": [
                {
                    "jpText": "なんかカッコ悪いな……",
                    "enText": "That's kind of uncool. ......",
                    "nextBlock": 24
                },
                {
                    "jpText": "なんかカッコ悪いね……",
                    "enText": "That's kind of uncool. ......",
                    "nextBlock": 24
                }
