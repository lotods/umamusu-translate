            "pathId": 4378606676788364932,
            "blockIdx": 1
        },
        {
            "jpName": "シンボリクリスエス",
            "enName": "Symboli Kris S",
            "jpText": "――そこのお前。\n――通達、だ。",
            "enText": "――You over there. \n――A notice for you.",
            "nextBlock": 3,
            "origClipLength": 125,
            "voiceIdx": 0,
            "animData": [
                {
                    "origLen": 195,
                    "pathId": -4930988111107629540
