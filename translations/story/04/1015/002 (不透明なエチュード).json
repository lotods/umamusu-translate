            "pathId": -803829048574764745,
            "blockIdx": 20
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "さあ、着いたよ！",
            "enText": "Here we are!",
            "nextBlock": 22,
            "origClipLength": 61,
            "voiceIdx": 14,
            "choices": [
                {
                    "jpText": "こんなところで何をするんだ……？",
                    "enText": "What are we <i>here</i> for……?",
                    "nextBlock": 22
                },
                {
                    "jpText": "こんなところで何をするの……？",
                    "enText": "What are we <i>here</i> for……?",
                    "nextBlock": 22
                }
            ],
            "animData": [
                {
                    "origLen": 99,
                    "pathId": 1846579867407484825
                }
            ],
            "pathId": 7220232345844269597,
            "blockIdx": 21
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "神社に来てすることなんて、決まっているじゃないか。",
            "enText": "Can't you tell by the fact we have come to a shrine?",
            "nextBlock": 23,
            "origClipLength": 120,
            "voiceIdx": 15,
            "animData": [
                {
                    "origLen": 29,
                    "pathId": -8040373823865363859
