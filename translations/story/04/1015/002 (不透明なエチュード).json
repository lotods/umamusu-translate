            "pathId": 7352979294721070655,
            "blockIdx": 22
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（チャリーン！）",
            "enText": "(<i>Ch-Ching!</i>)",
            "nextBlock": 24,
            "origClipLength": 55,
