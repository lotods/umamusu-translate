            "blockIdx": 23
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "ボクの美しさで\r\n世界に幸せが満ちあふれますように……！",
            "enText": "<i>May my beauty spread throughout the world \nand fill it with cheer and laughter…!</i>",
            "nextBlock": 25,
            "origClipLength": 137,
            "voiceIdx": 16,
            "pathId": 4104197182216128309,
            "blockIdx": 24
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "ボクの美しさで\r\n世界に幸せが満ちあふれますように……！",
            "enText": "<i>May my beauty spread throughout the world \nand fill it with cheer and laughter…!</i>",
            "nextBlock": 26,
            "origClipLength": 138,
            "voiceIdx": 17,
            "animData": [
                {
                    "origLen": 43,
                    "pathId": -6804990198234535544
