            "nextBlock": 5,
            "origClipLength": 128,
            "voiceIdx": 2,
            "choices": [
                {
                    "jpText": "カラオケに行くのか？",
                    "enText": "Going to karaoke?",
                    "nextBlock": 5
                },
                {
                    "jpText": "カラオケに行くの？",
                    "enText": "Going to karaoke?",
