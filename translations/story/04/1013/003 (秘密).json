    "text": [
        {
            "jpName": "メジロマックイーン",
            "enName": "",
            "jpText": "はっ……はっ……。\nまだ……まだ、鍛え足りない……！",
            "enText": "Haha…… haha…… Still…… not working out enough…… yet!",
            "nextBlock": 2,
            "origClipLength": 161,
            "animData": [
                {
                    "origLen": 115,
                    "pathId": 7183619284985448032
                },
                {
