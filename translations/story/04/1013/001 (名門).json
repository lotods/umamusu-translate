            "blockIdx": 14
        },
        {
            "jpName": "メジロマックイーン",
            "enName": "Mejiro McQueen",
            "jpText": "はっ、はっ……くっ……！！",
            "enText": "Hah, hah… guhh…!!",
            "nextBlock": 16,
            "origClipLength": 78,
            "animData": [
                {
                    "origLen": 198,
                    "pathId": -869801028880087179
