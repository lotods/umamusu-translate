                {
                    "jpText": "まずは落ち着いてくれ",
                    "enText": "Don't worry, take it easy.",
                    "nextBlock": 27
                },
                {
                    "jpText": "まずは落ち着いて",
                    "enText": "Don't worry, take it easy.",
                    "nextBlock": 27
                }
