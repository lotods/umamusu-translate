            "pathId": 911995602068320249,
            "blockIdx": 25
        },
        {
            "jpName": "カワカミプリンセス",
            "enName": "",
            "jpText": "へ……？",
            "enText": "She ……?",
            "nextBlock": 27,
            "origClipLength": 20,
            "animData": [
                {
                    "origLen": 52,
                    "pathId": 275355687825408019
                },
                {
                    "origLen": 100,
