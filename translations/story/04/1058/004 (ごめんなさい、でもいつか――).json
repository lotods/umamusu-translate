        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "最高の、レースに、するん、だぁあああああ～～～っ！",
            "enText": "Make it the best, race, \naaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaay!",
            "nextBlock": 6,
            "origClipLength": 243,
            "pathId": -118526754481824011,
            "blockIdx": 5
        },
        {
            "jpName": "メイショウドトウ",
            "enName": "Meisho Doto",
            "jpText": "――はひゃ！？",
            "enText": "―Hya!",
            "nextBlock": 7,
            "origClipLength": 46,
            "pathId": 1843838164424607431,
            "blockIdx": 6
        },
        {
            "jpName": "テイエムオペラオー",
            "enName": "T.M. Opera O",
            "jpText": "……ふっ！　いい仕上がりじゃないか、ドトウ！",
            "enText": "…… Phew! Nice finish, Doto!",
