            "nextBlock": 22,
            "origClipLength": 194,
            "choices": [
                {
                    "jpText": "頼ってもらえると嬉しいからな",
                    "enText": "I'm glad you're relying on me.",
                    "nextBlock": 22
                },
                {
                    "jpText": "頼ってもらえると嬉しいからね",
