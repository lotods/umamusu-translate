            "blockIdx": 13
        },
        {
            "jpName": "サトノダイヤモンド",
            "enName": "Satono Diamond",
            "jpText": "……行ってしまわれました……。",
            "enText": "…… has gone ……",
            "nextBlock": 15,
            "origClipLength": 74,
            "choices": [
                {
                    "jpText": "今の子は……",
                    "enText": "Now kids can go to ……",
                    "nextBlock": 15
