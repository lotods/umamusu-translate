            "pathId": 4689361853437130978,
            "blockIdx": 22
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "――数か月後。",
            "enText": "―A few months later.",
            "nextBlock": 24,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 242,
                    "pathId": -2512372556040759825
