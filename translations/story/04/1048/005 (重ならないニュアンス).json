        },
        {
            "jpName": "トーセンジョーダン",
            "enName": "Tosen Jordan",
            "jpText": "……は？",
            "enText": "What about ……?",
            "nextBlock": 30,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "『ホッピン・パンポピーン』！！！！",
                    "enText": "The Hoppin' Pumppin' ！！！！",
                    "nextBlock": 30
