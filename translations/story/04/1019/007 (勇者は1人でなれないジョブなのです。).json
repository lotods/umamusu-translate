            "pathId": 985345790386992289,
            "blockIdx": 12
        },
        {
            "jpName": "アグネスデジタル",
            "enName": "Agnes Digital",
            "jpText": "……ン゛ッ！！！　……ンン゛ッッ！！！！",
            "enText": "…Hngh!!! …HHHHnnngghhgghh!!!!",
            "nextBlock": 14,
            "origClipLength": 151,
            "voiceIdx": 11,
            "animData": [
                {
                    "origLen": 112,
                    "pathId": 379598455981947576
