            "nextBlock": 7,
            "origClipLength": 164,
            "voiceIdx": 5,
            "choices": [
                {
                    "jpText": "もっと語らなくていいのか？",
                    "enText": "Need to talk it off again?",
                    "nextBlock": 7
                },
                {
                    "jpText": "もっと語らなくていいの？",
                    "enText": "Need to talk it off again?",
                    "nextBlock": 7
                }
