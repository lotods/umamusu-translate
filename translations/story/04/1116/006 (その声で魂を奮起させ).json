            "blockIdx": 5
        },
        {
            "jpName": "ジェンティルドンナ",
            "enName": "Gentildonna",
            "jpText": "『殊勝な心掛け、ですこと』",
            "enText": "『Quite the commendable spirit』",
            "nextBlock": 7,
            "origClipLength": 112,
            "voiceIdx": 5,
            "choices": [
                {
                    "jpText": "『本気だぞ、トレーナーバッジに誓う！』",
                    "enText": "『I swear it on my trainer badge!』",
                    "nextBlock": 7
                },
                {
                    "jpText": "『本気だよ、トレーナーバッジに誓う！』",
                    "enText": "『I swear it on my trainer badge!』",
