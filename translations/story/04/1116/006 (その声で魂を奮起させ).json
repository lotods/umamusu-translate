            "pathId": 760322661821067907,
            "blockIdx": 2
        },
        {
            "jpName": "ジェンティルドンナ",
            "enName": "Gentildonna",
            "jpText": "故に――",
            "enText": "As such――",
            "nextBlock": 4,
            "origClipLength": 48,
            "voiceIdx": 2,
            "animData": [
                {
                    "origLen": 124,
                    "pathId": -3662570430806252731
