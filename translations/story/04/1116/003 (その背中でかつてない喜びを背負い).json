            "blockIdx": 30
        },
        {
            "jpName": "ヴィルシーナ",
            "enName": "Verxina",
            "jpText": "――……くっ、それでもっ！！",
            "enText": "――……Kuu, but even then!!",
            "nextBlock": 32,
            "origClipLength": 113,
            "voiceIdx": 24,
            "animData": [
                {
                    "origLen": 55,
