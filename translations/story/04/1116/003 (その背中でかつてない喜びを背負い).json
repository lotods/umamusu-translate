            "blockIdx": 12
        },
        {
            "jpName": "ジェンティルドンナ",
            "enName": "Gentildonna",
            "jpText": "…………ふ。",
            "enText": "…………Heh.",
            "nextBlock": 14,
            "origClipLength": 64,
            "voiceIdx": 12,
            "animData": [
