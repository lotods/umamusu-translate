            "pathId": -2341178159823816134,
            "blockIdx": 7
        },
        {
            "jpName": "ジェンティルドンナ",
            "enName": "Gentildonna",
            "jpText": "……こちら空いてるかしら？",
            "enText": "……Is this spot open?",
            "nextBlock": 9,
            "origClipLength": 103,
            "voiceIdx": 7,
            "animData": [
                {
                    "origLen": 58,
