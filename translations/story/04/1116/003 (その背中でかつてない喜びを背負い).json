            "pathId": -5884126147916765177,
            "blockIdx": 19
        },
        {
            "jpName": "ジェンティルドンナ",
            "enName": "Gentildonna",
            "jpText": "…………そう。",
            "enText": "…………I see.",
            "nextBlock": 21,
            "origClipLength": 63,
            "voiceIdx": 18,
            "animData": [
                {
                    "origLen": 230,
                    "pathId": 6104585956648555633
