            "blockIdx": 3
        },
        {
            "jpName": "ジェンティルドンナ",
            "enName": "Gentildonna",
            "jpText": "……これだけ？",
            "enText": "……Is this all?",
            "nextBlock": 5,
            "origClipLength": 68,
            "voiceIdx": 2,
            "choices": [
