            "blockIdx": 30
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "が、その一方で――",
            "enText": "However…",
            "nextBlock": 32,
            "origClipLength": 56,
            "voiceIdx": -1,
            "animData": [
                {
                    "origLen": 68,
                    "pathId": -2306687683577979729
