        },
        {
            "jpName": "カレンチャン",
            "enName": "",
            "jpText": "お姉ちゃんは……カレンを信じてないんだね。",
            "enText": "Your sister doesn't believe in…… Curren.",
            "nextBlock": 33,
            "origClipLength": 115,
            "choices": [
                {
                    "jpText": "えっ",
                    "enText": "What's up?",
                    "nextBlock": 33
                }
