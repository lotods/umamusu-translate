            "enName": "",
            "jpText": "ちらり。",
            "enText": "Glimpse.",
            "nextBlock": 7,
            "origClipLength": 29,
            "choices": [
                {
                    "jpText": "……よし、約束だ！",
                    "enText": "…… Okay, I promise!",
                    "nextBlock": 7
                },
                {
                    "jpText": "……よし、約束ね！",
