            "animData": [
                {
                    "origLen": 365,
                    "pathId": 8021342004036034827
                },
                {
                    "origLen": 56,
                    "pathId": -2906861733378048488
                }
            ],
            "pathId": 9015777854880154453,
            "blockIdx": 30
        },
        {
            "jpName": "クレープ屋の店長",
            "enName": "",
            "jpText": "え……？　な、なんですか、あなたは……。",
            "enText": "What, ……? What, what are you ……?",
            "nextBlock": 32,
