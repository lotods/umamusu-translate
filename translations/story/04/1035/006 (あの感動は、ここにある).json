            "origClipLength": 95,
            "animData": [
                {
                    "origLen": 75,
                    "pathId": -6625330064877194705
                }
            ],
            "pathId": 7406176946758430078,
            "blockIdx": 22
        },
        {
            "jpName": "ウイニングチケット",
            "enName": "Winning Ticket",
            "jpText": "……うんっ。\r\n美味しかったね、トレーナーさん！",
            "enText": "…… Yeah. That was delicious, Trainer!",
            "nextBlock": 24,
            "origClipLength": 90,
            "choices": [
                {
                    "jpText": "ああ、美味しかった。けど……",
