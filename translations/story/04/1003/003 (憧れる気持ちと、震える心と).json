            "pathId": -6034298251050880500,
            "blockIdx": 26
        },
        {
            "jpName": "ウマ娘たち",
            "enName": "Horsegirls",
            "jpText": "会長！　会長！　会長！！",
            "enText": "President! President! President!!",
            "nextBlock": 28,
            "origClipLength": 107,
