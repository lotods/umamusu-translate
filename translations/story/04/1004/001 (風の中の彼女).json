            "pathId": -187741440361818396,
            "blockIdx": 22
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "あらら～……。",
            "enText": "Oh dear～…",
            "nextBlock": 24,
            "origClipLength": 52,
