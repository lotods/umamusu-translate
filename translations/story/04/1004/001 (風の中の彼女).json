        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "――それから数日後の夕方。",
            "enText": "—One evening a few days later.",
            "nextBlock": 37,
            "origClipLength": 61,
            "pathId": -5810584982781492988,
            "blockIdx": 36
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "ふぅっ……。",
            "enText": "Pheeew…",
            "nextBlock": 38,
