            "pathId": -474421149517669906,
            "blockIdx": 14
        },
        {
            "jpName": "後輩ウマ娘B",
            "enName": "Junior Horsegirl B",
            "jpText": "こ、こんなタイム、初めて見ました……！",
            "enText": "I-I've never seen a time like this before…!",
            "nextBlock": 16,
            "origClipLength": 100,
            "animData": [
                {
                    "origLen": 111,
                    "pathId": -7359544616727746999
                },
                {
                    "origLen": 111,
                    "pathId": -9066254661881548102
                }
