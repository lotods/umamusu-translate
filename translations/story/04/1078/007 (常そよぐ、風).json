            "nextBlock": 3,
            "origClipLength": 151,
            "voiceIdx": 0,
            "choices": [
                {
                    "jpText": "おはよう、散歩の帰り？",
                    "enText": "Good morning. Did you go out for a walk?",
                    "nextBlock": 3
