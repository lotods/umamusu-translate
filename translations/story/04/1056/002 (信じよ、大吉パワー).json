        },
        {
            "jpName": "マチカネフクキタル",
            "enName": "Matikanefukukitaru",
            "jpText": "ううう～……！　関係なくないですっ！",
            "enText": "<i>Gueeeeh…!</i> My Great Luck still applies!",
            "nextBlock": 29,
            "origClipLength": 123,
            "voiceIdx": 22,
            "animData": [
                {
                    "origLen": 176,
                    "pathId": 8934819371354312923
                },
