            "pathId": 4517910357894157470,
            "blockIdx": 40
        },
        {
            "jpName": "ダイイチルビー",
            "enName": "Daiichi Ruby",
            "jpText": "…………。",
            "enText": "…………",
            "nextBlock": 42,
            "origClipLength": 42,
            "voiceIdx": 23,
            "choices": [
                {
                    "jpText": "は……計りたいな！！",
                    "enText": "Ah… I want to measure your time!!",
