            ],
            "pathId": 1013620886663096444,
            "blockIdx": 39
        },
        {
            "jpName": "ダイイチルビー",
            "enName": "Daiichi Ruby",
            "jpText": "特にはございません。",
            "enText": "There is nothing in particular.",
            "nextBlock": 41,
            "origClipLength": 54,
            "voiceIdx": 22,
            "choices": [
                {
                    "jpText": "タイムを計るとか！",
                    "enText": "How about measuring the time!",
                    "nextBlock": 41
                }
