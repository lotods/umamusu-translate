            "blockIdx": 7
        },
        {
            "jpName": "ニシノフラワー",
            "enName": "Nishino Flower",
            "jpText": "あ……。",
            "enText": "Oh, ……",
            "nextBlock": 9,
            "origClipLength": 52,
            "choices": [
                {
                    "jpText": "その脚は……",
                    "enText": "That leg can be found at ……",
                    "nextBlock": 9
                }
            ],
            "animData": [
                {
                    "origLen": 43,
                    "pathId": 4981099098145485499
                },
                {
                    "origLen": 29,
                    "pathId": -3516788100523665437
