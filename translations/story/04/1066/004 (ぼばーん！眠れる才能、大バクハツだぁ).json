            "blockIdx": 6
        },
        {
            "jpName": "ツインターボ",
            "enName": "Twin Turbo",
            "jpText": "だぁああああああああーっ！！",
            "enText": "Daaaaaaaaaaaaaaaa!!",
            "nextBlock": 8,
            "origClipLength": 81,
            "voiceIdx": 5,
            "pathId": 5297089894655663208,
            "blockIdx": 7
        },
        {
            "jpName": "ツインターボ",
            "enName": "Twin Turbo",
            "jpText": "ゴーーールっっ！！　あっ、トレーナー、お城が見えるぞ！\nおーい、お城ー！！！！！！",
            "enText": "GO～AL!! Ah, Trainer, I can see the castle! \nHey, castle!!!!!!",
            "nextBlock": 9,
            "origClipLength": 365,
            "voiceIdx": 6,
            "animData": [
                {
                    "origLen": 197,
                    "pathId": -793089648465229124
