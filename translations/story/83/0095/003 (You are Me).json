            "enName": "King Halo",
            "jpText": "あっ――",
            "enText": "Ah―",
            "nextBlock": 28,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 61,
                    "pathId": -441122804803288718
                },
                {
                    "origLen": 61,
