            "blockIdx": 2
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "！　…………えっと、その……。",
            "enText": "Oh my god!!!……… uh, its ……",
            "nextBlock": 4,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 45,
                    "pathId": 3188505391798643472
