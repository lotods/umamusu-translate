            "pathId": -3768419333247905921,
            "blockIdx": 13
        },
        {
            "jpName": "スカーレットの母親",
            "enName": "",
            "jpText": "え、ええ……。",
            "enText": "Yeah, yeah, ……",
            "nextBlock": 15,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 65,
                    "pathId": -771849636580122782
                },
                {
                    "origLen": 65,
