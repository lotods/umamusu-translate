            "nextBlock": 4,
            "origClipLength": 65,
            "voiceIdx": -1,
            "pathId": 6151750430744380337,
            "blockIdx": 3
        },
        {
            "jpName": "アイネスフウジン",
            "enName": "Ines Fujin",
            "jpText": "どいたどいたー！！",
