            "blockIdx": 2
        },
        {
            "jpName": "クラスメイトA",
            "enName": "Classmate A",
            "jpText": "あぁっ！　学級委員長っ！\r\nやっと見つけたぁ～っ！",
            "enText": "Aah! Class president! I finally found you!",
            "nextBlock": 4,
            "origClipLength": 77,
            "animData": [
                {
                    "origLen": 164,
                    "pathId": -563457011768527268
                },
                {
                    "origLen": 141,
