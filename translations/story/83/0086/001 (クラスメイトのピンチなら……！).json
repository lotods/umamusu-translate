            "animData": [
                {
                    "origLen": 77,
                    "pathId": -94991122751437851
                },
                {
                    "origLen": 101,
                    "pathId": -3576898079979304353
                }
            ],
            "pathId": -4644671514004410109,
            "blockIdx": 6
        },
        {
            "jpName": "クラスメイトA",
            "enName": "Classmate A",
            "jpText": "うんっ！　……あっ！　そこの\r\nトレーナーさんも来てくれませんかっ！？\n大人の人もいれば安心ですっ！",
            "enText": "Yup!…… Oh! Can you get the trainer there to \ncome too? I'd feel safer with an adult there!",
            "nextBlock": 8,
            "origClipLength": 110,
            "choices": [
                {
                    "jpText": "わ、わかった！",
                    "enText": "Wow, okay!",
                    "nextBlock": 8
