            "pathId": -720677092817133374,
            "blockIdx": 13
        },
        {
            "jpName": "ナリタトップロード",
            "enName": "Narita Top Road",
            "jpText": "ふ……ふふっ……あはは……っ！",
            "enText": "Huh…… Huh…… Hahaha ……!",
            "nextBlock": 15,
            "origClipLength": 70,
            "choices": [
                {
                    "jpText": "……！",
                    "enText": "……!",
                    "nextBlock": 15
