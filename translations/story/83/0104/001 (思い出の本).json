            ],
            "pathId": -7403612488776876837,
            "blockIdx": 7
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "ワッツッ！？　ロブロイ！？",
            "enText": "Watts! Rob Roy!",
            "nextBlock": 9,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 17,
                    "pathId": -9167690659701938144
                },
                {
                    "origLen": 72,
                    "pathId": -6668319591031106019
