            "pathId": 1838773102605318375,
            "blockIdx": 8
        },
        {
            "jpName": "シリウスシンボリ",
            "enName": "Sirius Symboli",
            "jpText": "おい、お前！",
            "enText": "Hey, you!",
            "nextBlock": 10,
            "origClipLength": 52,
            "choices": [
                {
                    "jpText": "は、はい！？",
                    "enText": "Ha, yes!",
                    "nextBlock": 10
                }
            ],
            "animData": [
                {
                    "origLen": 12,
                    "pathId": 4653537664035222011
                },
                {
                    "origLen": 63,
                    "pathId": 162338449899764599
