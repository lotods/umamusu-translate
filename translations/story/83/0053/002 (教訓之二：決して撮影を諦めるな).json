            "blockIdx": 27
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "『今を生きる』という意味の言葉デス！",
            "enText": "It's a word that means \"live in the now\"!",
            "nextBlock": 29,
            "origClipLength": 68,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 9212753010393203428
                },
                {
                    "origLen": 31,
