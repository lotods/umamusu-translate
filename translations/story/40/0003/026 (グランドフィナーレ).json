            "pathId": -7234301293928897180,
            "blockIdx": 21
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "数日が経過した。",
            "enText": "A few days passed.",
            "nextBlock": 23,
            "origClipLength": 55,
            "pathId": 8368853522233003275,
            "blockIdx": 22
