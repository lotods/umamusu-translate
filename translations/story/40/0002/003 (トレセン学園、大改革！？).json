            "pathId": -6486529264492873962,
            "blockIdx": 9
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Horsegirl A",
            "jpText": "ゆ、緩さ……？",
            "enText": "Yu, loose ……?",
            "nextBlock": 11,
            "origClipLength": 54,
            "voiceIdx": -1,
