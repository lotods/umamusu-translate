            "pathId": 6176303341890386870,
            "blockIdx": 14
        },
        {
            "jpName": "生徒たち",
            "enName": "Teachers",
            "jpText": "……！",
            "enText": "……!",
            "nextBlock": 16,
            "origClipLength": 48,
            "animData": [
