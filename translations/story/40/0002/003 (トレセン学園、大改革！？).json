        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "……！",
            "enText": "……!",
            "nextBlock": 30,
            "origClipLength": 48,
            "voiceIdx": 110,
            "animData": [
