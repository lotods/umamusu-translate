                {
                    "origLen": 60,
                    "pathId": 8156455452178212365
                },
                {
                    "origLen": 120,
                    "pathId": 599724386317115094
                }
            ],
            "pathId": 6721817062632617599,
            "blockIdx": 26
        },
        {
            "jpName": "ビターグラッセ",
            "enName": "Bitter Glasse",
            "jpText": "すまない、彼女のひどい物言いは\n気にしないでくれ。",
            "enText": "Sorry, don't mind her horrible language.",
            "nextBlock": 28,
            "origClipLength": 77,
            "voiceIdx": 65,
            "animData": [
                {
                    "origLen": 108,
                    "pathId": 905443321419066951
                },
                {
                    "origLen": 108,
                    "pathId": 4639637619488444974
                },
