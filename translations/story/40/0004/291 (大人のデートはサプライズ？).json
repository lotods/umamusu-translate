            "blockIdx": 10
        },
        {
            "jpName": "マヤノトップガン",
            "enName": "",
            "jpText": "ふあ！？",
            "enText": "Huh!",
            "nextBlock": 12,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 73,
                    "pathId": -1463003545430985751
                },
