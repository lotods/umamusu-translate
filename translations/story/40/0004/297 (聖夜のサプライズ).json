            "pathId": 5851496068712646852,
            "blockIdx": 23
        },
        {
            "jpName": "フジキセキ",
            "enName": "",
            "jpText": "――おや。",
            "enText": "―Oh.",
            "nextBlock": 25,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 55,
                    "pathId": 1067664066813403307
                },
                {
                    "origLen": 62,
                    "pathId": -7418378772704952002
