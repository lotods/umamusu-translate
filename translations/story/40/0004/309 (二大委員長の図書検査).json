        },
        {
            "jpName": "バンブーメモリー",
            "enName": "",
            "jpText": "ええっ！？　なんでっスか？",
            "enText": "What? What is it?",
            "nextBlock": 10,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 47,
                    "pathId": 6053692328105904056
