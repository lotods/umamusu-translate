            "blockIdx": 2
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "んん……。",
            "enText": "Hmmm……",
            "nextBlock": 4,
            "origClipLength": 61,
            "voiceIdx": 2,
            "animData": [
