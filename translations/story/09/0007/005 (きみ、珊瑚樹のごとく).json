            "blockIdx": 5
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "53ページ、ですか……？",
            "enText": "Page 53…?",
            "nextBlock": 7,
            "origClipLength": 86,
            "voiceIdx": 5,
            "animData": [
