            "blockIdx": 32
        },
        {
            "jpName": "シービー＆エース",
            "enName": "C.B. & Ace",
            "jpText": "――ふっ。\n――ははっ！",
            "enText": "Ha ha!",
            "nextBlock": 34,
            "origClipLength": 39,
            "voiceIdx": 31,
            "animData": [
                {
                    "origLen": 120,
                    "pathId": 1917607931144155334
                },
                {
                    "origLen": 63,
                    "pathId": 6621188321388257409
                },
                {
                    "origLen": 116,
                    "pathId": -2468276640504242401
