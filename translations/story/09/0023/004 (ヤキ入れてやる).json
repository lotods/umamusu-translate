            "pathId": -5107314692070277393,
            "blockIdx": 16
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "……は？",
            "enText": "…… What?",
            "nextBlock": 18,
            "origClipLength": 72,
            "voiceIdx": 16,
            "animData": [
                {
                    "origLen": 92,
                    "pathId": 3361284512701576016
                },
                {
                    "origLen": 93,
                    "pathId": -4551831803433592406
                },
