            "blockIdx": 7
        },
        {
            "jpName": "カツラギエース",
            "enName": "Katsuragi Ace",
            "jpText": "――ルドルフ！　シービー！",
            "enText": "Rudolf! C.B.!",
            "nextBlock": 9,
            "origClipLength": 62,
            "voiceIdx": 7,
            "animData": [
                {
                    "origLen": 93,
                    "pathId": 8480769740270526024
                },
