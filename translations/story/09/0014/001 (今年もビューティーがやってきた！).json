            "origClipLength": 185,
            "voiceIdx": 17,
            "animData": [
                {
                    "origLen": 89,
                    "pathId": 2177437906999401038
                },
                {
                    "origLen": 194,
                    "pathId": -6059322144985264744
                },
                {
                    "origLen": 175,
                    "pathId": -2861859564402316818
                }
