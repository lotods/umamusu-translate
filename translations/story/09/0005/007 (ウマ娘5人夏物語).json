            "pathId": -5548653212139896361,
            "blockIdx": 6
        },
        {
            "jpName": "スペシャルウィーク",
            "enName": "Special Week",
            "jpText": "……！",
            "enText": "……!",
            "nextBlock": 8,
            "origClipLength": 35,
            "animData": [
                {
                    "origLen": 89,
                    "pathId": 4876883899227244912
                },
                {
                    "origLen": 60,
                    "pathId": -5705008872893359826
