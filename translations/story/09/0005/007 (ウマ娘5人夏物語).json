            "pathId": -1115279890835766947,
            "blockIdx": 10
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "そんなわけで――",
            "enText": "And that's why―",
            "nextBlock": 12,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 119,
                    "pathId": 4590034591587868220
                },
                {
                    "origLen": 75,
