            "origClipLength": 249,
            "animData": [
                {
                    "origLen": 293,
                    "pathId": 2186708393835289929
                },
                {
                    "origLen": 156,
                    "pathId": 3827245839699715107
                },
                {
                    "origLen": 41,
                    "pathId": 3937731165886358454
