            "pathId": -7357924702770853418,
            "blockIdx": 9
        },
        {
            "jpName": "キング＆ネイチャ",
            "enName": "King & Nature",
            "jpText": "ありがとうございます！",
            "enText": "Thank you very much！",
            "nextBlock": 11,
            "origClipLength": 67,
            "animData": [
                {
                    "origLen": 50,
                    "pathId": -5456513618216716208
                },
                {
                    "origLen": 90,
                    "pathId": -5799962853827296313
                },
                {
                    "origLen": 20,
                    "pathId": 3338946890862464803
