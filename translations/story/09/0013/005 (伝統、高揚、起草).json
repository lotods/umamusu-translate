            "pathId": 3317042905434194277,
            "blockIdx": 8
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "あ……。",
            "enText": "Ah……",
            "nextBlock": 10,
            "origClipLength": 29,
            "animData": [
                {
                    "origLen": 89,
                    "pathId": -8401421409634990650
                },
