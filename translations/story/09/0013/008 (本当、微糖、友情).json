        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "あら、ネイチャさん。\n……。",
            "enText": "Oh, Nature-san. \n……",
            "nextBlock": 16,
            "origClipLength": 64,
            "voiceIdx": 14,
            "animData": [
