            "blockIdx": 1
        },
        {
            "jpName": "クラスメイトたち",
            "enName": "Classmates",
            "jpText": "は～いっ！",
            "enText": "Okaaa～aaay！",
            "nextBlock": 3,
            "origClipLength": 49,
            "animData": [
                {
                    "origLen": 332,
