            "blockIdx": 9
        },
        {
            "jpName": "一同",
            "enName": "All Together",
            "jpText": "よろしくお願いします！",
            "enText": "We look forward to your guidance!",
            "nextBlock": 11,
            "origClipLength": 59,
            "voiceIdx": 9,
            "animData": [
