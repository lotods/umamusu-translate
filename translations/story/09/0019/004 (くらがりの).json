            "pathId": 3906979883517164887,
            "blockIdx": 1
        },
        {
            "jpName": "一同",
            "enName": "",
            "jpText": "かんぱーいっ！！",
            "enText": "KANPAI!!!!",
            "nextBlock": 3,
            "origClipLength": 49,
            "animData": [
                {
                    "origLen": 60,
                    "pathId": 4179243663475405251
                },
