            "pathId": 3906979883517164887,
            "blockIdx": 1
        },
        {
            "jpName": "一同",
            "enName": "All Together",
            "jpText": "かんぱーいっ！！",
            "enText": "<i>To success!!</i>",
            "nextBlock": 3,
            "origClipLength": 49,
            "voiceIdx": 1,
            "animData": [
                {
                    "origLen": 60,
                    "pathId": 4179243663475405251
