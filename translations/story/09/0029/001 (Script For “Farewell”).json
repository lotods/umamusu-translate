            "pathId": 3617899548345405732,
            "blockIdx": 23
        },
        {
            "jpName": "シンボリクリスエス",
            "enName": "Symboli Kris S",
            "jpText": "――……。",
            "enText": "………",
            "nextBlock": 25,
            "origClipLength": 29,
