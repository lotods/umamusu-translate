            "pathId": -8515288745640259902,
            "blockIdx": 19
        },
        {
            "jpName": "スペシャルウィーク",
            "enName": "Special Week",
            "jpText": "っ、……へ？　風……？",
            "enText": "っ……へ? Wind ……?",
            "nextBlock": 21,
            "origClipLength": 95,
            "animData": [
                {
                    "origLen": 127,
                    "pathId": -4598281051317692939
                }
